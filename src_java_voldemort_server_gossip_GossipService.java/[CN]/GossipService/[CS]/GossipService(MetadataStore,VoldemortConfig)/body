{
  super(ServiceType.GOSSIP);
  executor=Executors.newSingleThreadExecutor();
  ClientConfig clientConfig=new ClientConfig().setMaxConnectionsPerNode(1).setMaxThreads(1).setConnectionTimeout(voldemortConfig.getAdminConnectionTimeout(),TimeUnit.MILLISECONDS).setSocketTimeout(voldemortConfig.getSocketTimeoutMs(),TimeUnit.MILLISECONDS).setSocketBufferSize(voldemortConfig.getAdminSocketBufferSize());
  AdminClient adminClient=new ProtoBuffAdminClientRequestFormat(metadataStore.getCluster(),clientConfig);
  gossiper=new Gossiper(metadataStore,adminClient,GOSSIP_INTERVAL);
}
