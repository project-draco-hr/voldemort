{
  super(ServiceType.HTTP);
  this.port=httpPort;
  this.numberOfThreads=numberOfThreads;
  this.server=server;
  this.velocityEngine=new VelocityEngine(VoldemortServletContextListener.VOLDEMORT_TEMPLATE_DIR);
  this.requestHandler=new SocketRequestHandlerFactory(server.getStoreRepository(),server.getMetadataStore(),server.getVoldemortConfig(),server.getAsyncRunner(),null).getRequestHandler(requestType);
}
