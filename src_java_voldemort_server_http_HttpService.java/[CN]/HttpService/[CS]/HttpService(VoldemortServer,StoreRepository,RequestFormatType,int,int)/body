{
  super(ServiceType.HTTP);
  this.port=httpPort;
  this.numberOfThreads=numberOfThreads;
  this.server=server;
  this.velocityEngine=new VelocityEngine(VoldemortServletContextListener.VOLDEMORT_TEMPLATE_DIR);
  this.requestHandler=new RequestHandlerFactory(storeRepository,server.getVoldemortMetadata(),server.getVoldemortConfig()).getRequestHandler(requestType);
}
