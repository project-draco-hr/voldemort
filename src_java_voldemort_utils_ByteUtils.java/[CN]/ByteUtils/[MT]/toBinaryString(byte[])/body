{
  StringBuffer buffer=new StringBuffer();
  for (  byte b : bytes) {
    String bin=Integer.toBinaryString(0xFF & b);
    bin=bin.substring(0,Math.min(bin.length(),8));
    for (int j=0; j < 8 - bin.length(); j++) {
      buffer.append('0');
    }
    buffer.append(bin);
  }
  return buffer.toString();
}
