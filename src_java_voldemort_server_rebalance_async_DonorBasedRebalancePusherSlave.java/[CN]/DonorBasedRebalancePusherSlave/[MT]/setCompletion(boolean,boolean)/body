{
  if (!getStatus().isComplete()) {
    try {
      if (notifySlave) {
        queue.put(END);
      }
    }
 catch (    InterruptedException e) {
      logger.info("Unable to send termination message to pusher slave for node " + nodeId + " due to the following reason: "+ e.getMessage());
    }
 finally {
      if (immediateTerminate) {
        markComplete();
        notifyAll();
      }
    }
  }
}
