{
  while (null == currentElem) {
    try {
      currentElem=getNextElem();
    }
 catch (    InterruptedException e) {
      logger.info("hasNext is interrupted while waiting for the next elem.");
    }
  }
  if (currentElem != null && currentElem.equals(END)) {
    return false;
  }
 else {
    return true;
  }
}
