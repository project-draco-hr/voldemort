{
synchronized (rebalancePermitMap) {
    if (!rebalancePermitMap.containsKey(donorNodeId))     rebalancePermitMap.put(donorNodeId,new AtomicBoolean(false));
    AtomicBoolean rebalancePermit=rebalancePermitMap.get(donorNodeId);
    if (rebalancePermit.compareAndSet(false,true))     return true;
  }
  return false;
}
