{
  StringBuilder builder=new StringBuilder("{");
  boolean firstLine=true;
  for (  Entry<String,String> entry : this.props.entrySet()) {
    if (oneLinePerEntry) {
      builder.append("\n\t");
    }
 else {
      if (firstLine) {
        firstLine=false;
      }
 else {
        builder.append(", ");
      }
    }
    builder.append(entry.getKey());
    builder.append(": ");
    builder.append(entry.getValue());
  }
  if (oneLinePerEntry) {
    builder.append("\n");
  }
  builder.append("}");
  return builder.toString();
}
