{
  if ("int8".equals(keyType))   return Byte.valueOf(key);
 else   if ("int16".equals(keyType))   return Short.valueOf(key);
 else   if ("int32".equals(keyType))   return Integer.valueOf(key);
 else   if ("int64".equals(keyType))   return Long.valueOf(key);
 else   if ("string".equals(keyType))   return key;
 else   throw new IllegalArgumentException("Unsupported key type: " + keyType);
}
