{
  resetIfNeeded();
  long index=0;
  if (data >= this.upperBound) {
    index=nBuckets - 1;
  }
 else   if (data < 0) {
    logger.error(data + " can't be bucketed because it is negative!");
    return;
  }
 else {
    index=data / step;
  }
  if (index >= nBuckets) {
    logger.error(data + " can't be bucketed because index overflowed number of buckets!");
    return;
  }
  buckets[(int)index]++;
  sum+=data;
  size++;
}
