{
  testPutRequest(new ByteArray(),new byte[0],new VectorClock(),null);
  testPutRequest(TestUtils.toByteArray("hello"),"world".getBytes(),new VectorClock(),null);
  this.store.put(TestUtils.toByteArray("hello"),new Versioned<byte[]>("world".getBytes(),new VectorClock()));
  testPutRequest(TestUtils.toByteArray("hello"),"world".getBytes(),new VectorClock(),ObsoleteVersionException.class);
}
