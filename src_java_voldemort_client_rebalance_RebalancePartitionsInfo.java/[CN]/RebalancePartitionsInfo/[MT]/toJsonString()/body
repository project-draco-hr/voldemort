{
  Map map=ImmutableMap.builder().put("stealerId",stealerId).put("donorId",donorId).put("partitionList",partitionList).put("unbalancedStoreList",unbalancedStoreList).put("deleteDonorPartitions",deleteDonorPartitions).put("attempt",attempt).build();
  StringWriter writer=new StringWriter();
  new JsonWriter(writer).write(map);
  writer.flush();
  return writer.toString();
}
