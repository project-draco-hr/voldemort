{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  RebalancePartitionsInfo that=(RebalancePartitionsInfo)o;
  if (donorId != that.donorId)   return false;
  if (stealerId != that.stealerId)   return false;
  if (!initialCluster.equals(that.initialCluster))   return false;
  if (storeToReplicaToAddPartitionList != null ? !storeToReplicaToAddPartitionList.equals(that.storeToReplicaToAddPartitionList) : that.storeToReplicaToAddPartitionList != null)   return false;
  return true;
}
