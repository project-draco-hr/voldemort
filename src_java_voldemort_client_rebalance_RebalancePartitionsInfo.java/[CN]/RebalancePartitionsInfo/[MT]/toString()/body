{
  StringBuffer sb=new StringBuffer();
  sb.append("RebalancingStealInfo(" + getStealerId() + " <--- "+ getDonorId()+ ", partitions moved ");
  for (int replicaNum=0; replicaNum <= maxReplica; replicaNum++) {
    if (replicaToPartitionList.containsKey(replicaNum))     sb.append(" - " + replicaToPartitionList.get(replicaNum));
 else     sb.append(" - []");
  }
  sb.append(", stores: " + getUnbalancedStoreList() + ", delete - "+ deletePartitions+ ")");
  return sb.toString();
}
