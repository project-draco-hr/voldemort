{
  AdminClient adminClient=RebalanceUtils.createTempAdminClient(voldemortConfig,metadata.getCluster(),voldemortConfig.getClientMaxConnectionsPerNode());
  try {
    Versioned<Cluster> latestCluster=RebalanceUtils.getLatestCluster(new ArrayList<Integer>(),adminClient);
    metadata.put(MetadataStore.CLUSTER_KEY,latestCluster.getValue());
    checkAndAddNodeStore();
    routedStore.updateRoutingStrategy(metadata.getRoutingStrategy(getName()));
  }
  finally {
    adminClient.close();
  }
}
