{
  try {
    this.fatClientExecutor.submit(new HttpGetAllRequestExecutor(getAllRequestObject,getAllRequestMessageEvent,this.dynamicTimeoutClient,storeName));
    if (logger.isDebugEnabled()) {
      logger.debug("Submitted a get all request");
    }
  }
 catch (  RejectedExecutionException rej) {
    handleRejectedException(getAllRequestMessageEvent);
  }
}
