{
  try {
    this.fatClientExecutor.submit(new HttpPutRequestExecutor(putRequestObject,putRequestMessageEvent,this.dynamicTimeoutClient,startTimestampInNs,this.coordinatorPerfStats));
    if (logger.isDebugEnabled()) {
      logger.debug("Submitted a put request");
    }
  }
 catch (  RejectedExecutionException rej) {
    handleRejectedException(rej,putRequestMessageEvent);
  }
}
