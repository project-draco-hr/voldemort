{
  try {
    this.fatClientExecutor.submit(new HttpGetRequestExecutor(getRequestObject,getRequestMessageEvent,this.dynamicTimeoutClient,startTimestampInNs,this.coordinatorPerfStats));
    if (logger.isDebugEnabled()) {
      logger.debug("Submitted a get request");
    }
  }
 catch (  RejectedExecutionException rej) {
    handleRejectedException(rej,getRequestMessageEvent);
  }
}
