{
  try {
    this.fatClientExecutor.submit(new GetSchemataRequestExecutor(getRequestMessageEvent,storeName,storeClientFactory));
    if (logger.isDebugEnabled()) {
      logger.debug("Submitted a get schemata request");
    }
  }
 catch (  RejectedExecutionException rej) {
    handleRejectedException(rej,getRequestMessageEvent);
  }
}
