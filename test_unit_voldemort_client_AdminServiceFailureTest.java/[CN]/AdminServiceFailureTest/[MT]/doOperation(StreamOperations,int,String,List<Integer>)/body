{
switch (e) {
case DELETE_PARTITIONS:
    putAlltoStore();
  getAdminClient().deletePartitions(nodeId,storeName,partitionList,null);
return;
case FETCH_ENTRIES:
putAlltoStore();
consumeIterator(getAdminClient().fetchEntries(nodeId,storeName,partitionList,null));
return;
case FETCH_KEYS:
putAlltoStore();
consumeIterator(getAdminClient().fetchKeys(nodeId,storeName,partitionList,null));
return;
case UPDATE_ENTRIES:
getAdminClient().updateEntries(nodeId,storeName,getRandomlyFailingIterator(ServerTestUtils.createRandomKeyValuePairs(TEST_KEYS)),null);
return;
default :
throw new RuntimeException("Unknown operation");
}
}
