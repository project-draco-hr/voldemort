{
  StoreDefinition beforeStoreDef=ServerTestUtils.getStoreDef("test1",1,1,1,1,1,RoutingStrategyType.CONSISTENT_STRATEGY);
  StoreDefinition afterStoreDef=ServerTestUtils.getStoreDef("test1",2,2,2,2,2,RoutingStrategyType.CONSISTENT_STRATEGY);
  test(consistentRoutingCluster,consistentRoutingClusterModified,Lists.newArrayList(beforeStoreDef),Lists.newArrayList(afterStoreDef));
  beforeStoreDef=ServerTestUtils.getStoreDef("test1",2,2,2,2,2,RoutingStrategyType.CONSISTENT_STRATEGY);
  afterStoreDef=ServerTestUtils.getStoreDef("test1",3,3,3,3,3,RoutingStrategyType.CONSISTENT_STRATEGY);
  test(consistentRoutingCluster,consistentRoutingClusterModified,Lists.newArrayList(beforeStoreDef),Lists.newArrayList(afterStoreDef));
}
