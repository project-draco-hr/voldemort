{
  StoreDefinition beforeStoreDef=ServerTestUtils.getStoreDef("test1",1,1,1,1,1,RoutingStrategyType.CONSISTENT_STRATEGY);
  StoreDefinition afterStoreDef=ServerTestUtils.getStoreDef("test1",2,1,1,1,1,RoutingStrategyType.CONSISTENT_STRATEGY);
  test(consistentRoutingCluster,consistentRoutingClusterModified,Lists.newArrayList(beforeStoreDef),Lists.newArrayList(afterStoreDef));
  beforeStoreDef=ServerTestUtils.getStoreDef("test1",2,1,1,1,1,RoutingStrategyType.CONSISTENT_STRATEGY);
  afterStoreDef=ServerTestUtils.getStoreDef("test1",3,1,1,1,1,RoutingStrategyType.CONSISTENT_STRATEGY);
  test(consistentRoutingCluster,consistentRoutingClusterModified,Lists.newArrayList(beforeStoreDef),Lists.newArrayList(afterStoreDef));
}
