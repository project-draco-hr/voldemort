{
  List<Versioned<String>> values=innerStore.get(key);
  if (values.size() > 1)   throw new VoldemortException("Inconsistent metadata found: expected 1 version but found " + values.size() + " for key:"+ key);
  if (values.size() > 0) {
    String valueStr=values.get(0).getValue();
    return new Versioned<byte[]>(ByteUtils.getBytes(valueStr,"UTF-8"),values.get(0).getVersion());
  }
  throw new VoldemortException("No metadata found for required key:" + key);
}
