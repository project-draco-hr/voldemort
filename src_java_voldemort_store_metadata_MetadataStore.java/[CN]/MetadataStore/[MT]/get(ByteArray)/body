{
  String keyStr=ByteUtils.getString(key.get(),"UTF-8");
  if (CLUSTER_KEY.equals(keyStr))   return Collections.singletonList(new Versioned<byte[]>(ByteUtils.getBytes(clusterMapper.writeCluster(cluster),"UTF-8")));
 else   if (STORES_KEY.equals(keyStr))   return Collections.singletonList(new Versioned<byte[]>(ByteUtils.getBytes(storeMapper.writeStoreList(storeDefs),"UTF-8")));
 else   throw new VoldemortException("Unknown metadata key " + keyStr);
}
