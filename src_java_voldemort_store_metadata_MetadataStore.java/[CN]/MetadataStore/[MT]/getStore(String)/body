{
  List<StoreDefinition> storeDefs=storeMapper.readStoreList(new StringReader(getSingleValue(get(new ByteArray(ByteUtils.getBytes(STORES_KEY,"UTF-8"))))));
  for (  StoreDefinition storeDef : storeDefs) {
    if (storeName.equals(storeDef.getName()))     return storeDef;
  }
  throw new VoldemortException("Store " + storeName + " not found in MetadataStore");
}
