{
  Versioned<Cluster> currentVersionedCluster=RebalanceUtils.getLatestCluster(RebalanceUtils.getNodeIds(Lists.newArrayList(adminClient.getAdminClientCluster().getNodes())),adminClient);
  Cluster currentCluster=currentVersionedCluster.getValue();
  validateClusterState(currentCluster);
  rebalance(currentCluster,targetCluster);
}
