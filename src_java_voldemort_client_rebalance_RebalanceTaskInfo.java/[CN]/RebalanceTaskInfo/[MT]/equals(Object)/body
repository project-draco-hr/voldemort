{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  RebalanceTaskInfo that=(RebalanceTaskInfo)o;
  if (donorId != that.donorId)   return false;
  if (stealerId != that.stealerId)   return false;
  if (!initialCluster.equals(that.initialCluster))   return false;
  if (storeToPartitionIds != null ? !storeToPartitionIds.equals(that.storeToPartitionIds) : that.storeToPartitionIds != null)   return false;
  return true;
}
