{
  if (logger.isInfoEnabled()) {
    long totalTimeS=(System.currentTimeMillis() - startTimeMs) / Time.MS_PER_SECOND;
    logger.info(tag + " : scanned " + scanned+ " and fetched "+ fetched+ " for store '"+ storageEngine.getName()+ "' replicaToPartitionList:"+ replicaToPartitionList+ " in "+ totalTimeS+ " s");
  }
}
