{
  for (  Node node : cluster.getNodes()) {
    Store<ByteArray,byte[]> store;
    if (node.getId() != localNode) {
      store=new SocketStore(def.getName(),new SocketDestination(node.getHost(),node.getSocketPort(),voldemortConfig.getRequestFormatType()),socketPool,false,true);
      ;
      this.storeRepository.addRedirectingSocketStore(node.getId(),store);
    }
  }
}
