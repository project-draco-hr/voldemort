{
  List<StoreDefinition> defs=new ArrayList<StoreDefinition>();
  SerializerDefinition serDef=new SerializerDefinition("string");
  for (int i=0; i < numStores; i++)   defs.add(new StoreDefinition("test" + i,InMemoryStorageConfiguration.TYPE_NAME,serDef,serDef,RoutingTier.SERVER,2,1,1,1,1,1));
