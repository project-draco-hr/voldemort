{
  Cluster cluster=ServerTestUtils.getLocalCluster(numServers,partitionMap);
  for (int i=0; i < numServers; i++) {
    voldemortServers[i]=ServerTestUtils.startVoldemortServer(socketStoreFactory,ServerTestUtils.createServerConfig(useNio,i,TestUtils.createTempDir().getAbsolutePath(),clusterFile,storeFile,properties),cluster);
  }
  return cluster;
}
