{
  Cluster currentCluster=ServerTestUtils.getLocalCluster(2,new int[][]{{1,2,3,4},{5,6,7,8}});
  Cluster targetCluster=ServerTestUtils.getLocalCluster(3,new int[][]{{1,2},{5,6,8},{3,4,7}});
  Map<Integer,Map<Integer,List<Integer>>> map=RebalanceUtils.getStealPartitionsMap(currentCluster,targetCluster);
  checkStealPartitions(2,map,new int[][]{{3,4},{7}});
  System.out.println(RebalanceUtils.getStealPartitionsMapAsString(map));
}
