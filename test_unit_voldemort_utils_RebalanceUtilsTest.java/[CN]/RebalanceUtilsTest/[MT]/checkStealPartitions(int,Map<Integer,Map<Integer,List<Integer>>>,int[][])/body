{
  assertEquals("NodeId should be present in stealMap",true,stealPartitionsMap.containsKey(nodeId));
  Map<Integer,List<Integer>> nodeStealMap=stealPartitionsMap.get(nodeId);
  for (int i=0; i < nodeToStealPartition.length; i++)   if (nodeToStealPartition[i].length > 0) {
    assertEquals("NodeId should be present in stealMap",true,nodeStealMap.containsKey(i));
    for (    int p : nodeToStealPartition[i]) {
      assertEquals("steal partition should be present",true,nodeStealMap.get(i).contains(p));
    }
  }
}
