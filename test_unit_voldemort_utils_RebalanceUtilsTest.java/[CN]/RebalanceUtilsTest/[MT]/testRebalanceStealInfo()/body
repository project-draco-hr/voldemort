{
  RebalancePartitionsInfo info=new RebalancePartitionsInfo(0,1,Arrays.asList(1,2,3,4),Arrays.asList("test1","test2"),false,0);
  System.out.println("info:" + info.toString());
  assertEquals("RebalanceStealInfo fromString --> toString should match.",info.toString(),(new RebalancePartitionsInfo(info.toJsonString())).toString());
}
