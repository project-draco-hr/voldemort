{
  for (  RebalancePartitionsInfo rebalanceInfo : rebalanceInfoList) {
    boolean match=false;
    for (    RebalancePartitionsInfo nodeRebalanceInfo : nodePlan.getRebalanceTaskList()) {
      if (rebalanceInfo.getDonorId() == nodeRebalanceInfo.getDonorId()) {
        assertEquals("partitions should match",true,rebalanceInfo.getPartitionList().containsAll(nodeRebalanceInfo.getPartitionList()));
        assertEquals("delete partitions should match",true,rebalanceInfo.getDeletePartitionsList().containsAll(nodeRebalanceInfo.getDeletePartitionsList()));
        assertEquals("store list should match",true,rebalanceInfo.getUnbalancedStoreList().containsAll(nodeRebalanceInfo.getUnbalancedStoreList()));
        match=true;
      }
    }
    assertNotSame("rebalancePartition Info " + rebalanceInfo + " should be present in the nodePlan "+ nodePlan.getRebalanceTaskList(),false,match);
  }
}
