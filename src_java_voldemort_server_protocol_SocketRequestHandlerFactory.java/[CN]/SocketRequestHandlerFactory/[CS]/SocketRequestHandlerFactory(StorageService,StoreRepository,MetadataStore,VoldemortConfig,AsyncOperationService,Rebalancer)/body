{
  this.storage=storageService;
  this.repository=repository;
  this.metadata=metadata;
  this.voldemortConfig=voldemortConfig;
  this.asyncService=asyncService;
  this.rebalancer=rebalancer;
  this.stats=new StreamStats();
  if (null != voldemortConfig && voldemortConfig.isJmxEnabled())   JmxUtils.registerMbean(new StreamStatsJmx(stats),JmxUtils.createObjectName(metadata.getCluster().getName() + ".voldemort.store.stats","admin-streaming"));
}
