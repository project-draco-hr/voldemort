{
  if (iteration < maxCacheDepth) {
    if (keyCache[cacheIndex] == null) {
      readFrom(index,indexByteOffset,foundKey);
      keyCache[cacheIndex]=ByteUtils.copy(foundKey,0,foundKey.length);
    }
    return keyCache[cacheIndex];
  }
 else {
    readFrom(index,indexByteOffset,foundKey);
    return foundKey;
  }
}
