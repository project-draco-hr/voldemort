{
  RandomAccessFile f=null;
  try {
    f=files.poll(waitTimeoutMs,TimeUnit.MILLISECONDS);
    return f.length();
  }
 catch (  IOException e) {
    throw new VoldemortException(e);
  }
catch (  InterruptedException e) {
    throw new VoldemortException(e);
  }
 finally {
    if (f != null)     files.add(f);
  }
}
