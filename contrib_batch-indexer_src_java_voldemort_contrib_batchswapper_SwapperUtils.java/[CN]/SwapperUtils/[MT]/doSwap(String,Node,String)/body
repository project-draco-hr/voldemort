{
  String data=URLEncoder.encode("operation","UTF-8") + "=" + URLEncoder.encode("swap","UTF-8");
  data+="&" + URLEncoder.encode("index","UTF-8") + "="+ URLEncoder.encode(getIndexDestinationFile(node.getId(),destinationDir),"UTF-8");
  data+="&" + URLEncoder.encode("data","UTF-8") + "="+ URLEncoder.encode(getDataDestinationFile(node.getId(),destinationDir),"UTF-8");
  data+="&" + URLEncoder.encode("store","UTF-8") + "="+ URLEncoder.encode(storeName,"UTF-8");
  URL url=new URL("http://" + node.getHost() + ":"+ node.getHttpPort()+ "/read-only/mgmt");
  System.out.println("swapping node:" + node.getId() + " with url:"+ url.toString()+ " data:"+ data);
  HttpURLConnection connection=(HttpURLConnection)url.openConnection();
  connection.setRequestMethod("POST");
  connection.setRequestProperty("Content-Type","application/x-www-form-urlencoded");
  connection.setRequestProperty("Content-Length","" + Integer.toString(data.getBytes().length));
  connection.setRequestProperty("Content-Language","en-US");
  connection.setUseCaches(false);
  connection.setDoInput(true);
  connection.setDoOutput(true);
  OutputStreamWriter wr=new OutputStreamWriter(connection.getOutputStream());
  wr.write(data);
  wr.flush();
  wr.close();
  InputStream is=connection.getInputStream();
  BufferedReader rd=new BufferedReader(new InputStreamReader(is));
  String line;
  StringBuffer response=new StringBuffer();
  while ((line=rd.readLine()) != null) {
    response.append(line);
    response.append('\r');
  }
  System.out.println("doSwap Completed for " + node + "  Response:"+ response.toString());
  rd.close();
}
