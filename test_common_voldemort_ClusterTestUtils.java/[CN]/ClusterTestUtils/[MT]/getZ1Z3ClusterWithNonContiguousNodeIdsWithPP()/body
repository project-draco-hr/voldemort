{
  int zoneIds[]=new int[]{1,3};
  int nodesPerZone[][]=new int[][]{{3,4,5,12},{9,10,11,14}};
  int partitionMap[][]=new int[][]{{0,6,12,16,17},{7,13},{8,14},{1,2},{9,15},{10},{5,11},{3,4}};
  return ServerTestUtils.getLocalNonContiguousZonedCluster(zoneIds,nodesPerZone,partitionMap,getClusterPorts());
}
