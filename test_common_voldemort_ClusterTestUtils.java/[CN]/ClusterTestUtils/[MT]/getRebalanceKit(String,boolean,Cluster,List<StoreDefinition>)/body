{
  RebalanceController rebalanceController=new RebalanceController(bootstrapUrl,RebalanceController.MAX_PARALLEL_REBALANCING,stealerBased,REBALANCE_CONTROLLER_TEST_PROXY_PAUSE_IN_SECONDS);
  RebalancePlan rebalancePlan=rebalanceController.getPlan(finalCluster,finalStoreDefs,RebalancePlan.BATCH_SIZE);
  return new RebalanceKit(rebalanceController,rebalancePlan);
}
