{
  int zoneIds[]=new int[]{1,3,5};
  int nodesPerZone[][]=new int[][]{{3,4,5,18},{9,10,11,19},{15,16,17,20}};
  int partitionMap[][]=new int[][]{{0,9},{1,15},{2,11,7},{},{12,16,3,6},{13,8,17,10},{5,4,14},{},{18,20},{19,21},{22,23,24},{}};
  return ServerTestUtils.getLocalNonContiguousZonedCluster(zoneIds,nodesPerZone,partitionMap,getClusterPorts());
}
