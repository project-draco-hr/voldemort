{
  int zoneIds[]=new int[]{1,3,5};
  int nodesPerZone[][]=new int[][]{{3,4,5},{9,10,11},{15,16,17}};
  int partitionMap[][]=new int[][]{{0,9,6,17,1,2},{10,15},{11,7},{3,12,16,4,5},{13,8},{14},{18,20,19,22},{21},{23,24}};
  return ServerTestUtils.getLocalNonContiguousZonedCluster(zoneIds,nodesPerZone,partitionMap,getClusterPorts());
}
