{
  long startTimeNs=0;
  boolean succeeded=false;
  if (logger.isDebugEnabled()) {
    startTimeNs=time.getNanoseconds();
  }
  try {
    getInnerStore().put(key,value);
    succeeded=true;
  }
  finally {
    if (logger.isDebugEnabled()) {
      double elapsedMs=(time.getNanoseconds() - startTimeNs) / (double)Time.NS_PER_MS;
      logger.debug("PUT from store '" + getName() + "' completed "+ (succeeded ? "successfully" : "unsuccessfully")+ " in "+ elapsedMs+ " ms.");
    }
  }
}
