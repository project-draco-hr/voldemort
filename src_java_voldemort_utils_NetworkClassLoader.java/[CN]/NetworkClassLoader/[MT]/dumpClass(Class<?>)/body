{
  String filename=cl.getName().replace('.',File.separatorChar) + ".class";
  InputStream in=null;
  logger.debug("NetworkClassloader dumpClass() :" + cl.getCanonicalName());
  try {
    in=this.getResourceAsStream(filename);
    byte[] classBytes=IOUtils.toByteArray(in);
    return classBytes;
  }
  finally {
    if (null != in)     in.close();
  }
}
