{
  this.name=Utils.notNull(name);
  SegmentFactory segmentFactory=new MemorySegmentFactory();
  try {
    datastore=new DynamicDataStore(dataDirectory,initLevel,segmentFileSizeMB,segmentFactory,hashLoadFactor);
  }
 catch (  Exception e) {
    logger.error("Failed to initialize datastore");
    datastore=null;
  }
}
