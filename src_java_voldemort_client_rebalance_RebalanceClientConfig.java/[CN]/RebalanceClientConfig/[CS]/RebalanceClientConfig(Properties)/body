{
  super(properties);
  Props props=new Props(properties);
  if (properties.containsKey(CLUSTER_MAJORITY_THRESHOLD_PERCENTAGE))   this.setClusterMajorityThresholdPercentage(props.getDouble(CLUSTER_MAJORITY_THRESHOLD_PERCENTAGE));
  if (properties.containsKey(MAX_PARALLEL_REBALANCING_NODES))   this.setMaxParallelRebalancingNodes(props.getInt(MAX_PARALLEL_REBALANCING_NODES));
}
