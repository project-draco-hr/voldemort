{
  Map<Integer,NonblockingStore> nonblockingStores=Maps.newHashMap();
  for (  Map.Entry<Integer,Store<ByteArray,byte[]>> entry : nodeStores.entrySet())   nonblockingStores.put(entry.getKey(),toNonblockingStore(entry.getValue()));
  return create(cluster,storeDefinition,nodeStores,nonblockingStores,null,repairReads,Zone.DEFAULT_ZONE_ID,failureDetector);
}
