{
  List<String> commands=new ArrayList<String>();
  boolean isInQuotes=false;
  int start=0;
  for (int i=0; i < command.length(); i++) {
    char c=command.charAt(i);
    if (c == '\"') {
      isInQuotes=!isInQuotes;
    }
 else     if (c == ' ' && !isInQuotes) {
      String substring=command.substring(start,i).trim();
      start=i + 1;
      if (substring.trim().length() > 0)       commands.add(substring.replace("\"",""));
    }
  }
  String substring=command.substring(start).trim();
  if (substring.length() > 0)   commands.add(substring.replace("\"",""));
  return commands;
}
