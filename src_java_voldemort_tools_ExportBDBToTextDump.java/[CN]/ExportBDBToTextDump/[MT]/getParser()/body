{
  OptionParser parser=new OptionParser();
  parser.acceptsAll(Arrays.asList("bdb"),"Store level BDB folder").withRequiredArg().ofType(String.class).describedAs("input-file-or-folder");
  parser.acceptsAll(Arrays.asList("o","output"),"Output folder of text dump").withRequiredArg().ofType(String.class).describedAs("output-folder");
  return parser;
}
