{
  String putConfigAvro="{\"test\": {\"connection_timeout_ms\": \"1111\", \"socket_timeout_ms\": \"1024\"}}";
  String getConfigAvro1, getConfigAvro2;
  getConfigAvro1=adminClient.getStoreClientConfigString(Arrays.asList(STORE_NAME),ADMIN_URL);
  try {
    CoordinatorAdminCommand.executeCommand(new String[]{"delete","-s",STORE_NAME,"-u",ADMIN_URL,"--confirm"});
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  getConfigAvro2=adminClient.getStoreClientConfigString(Arrays.asList(STORE_NAME),ADMIN_URL);
  assertFalse(ClientConfigUtil.compareMultipleClientConfigAvro(getConfigAvro1,getConfigAvro2));
  try {
    CoordinatorAdminCommand.executeCommand(new String[]{"put","-d",putConfigAvro,"-u",ADMIN_URL,"--confirm"});
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  getConfigAvro1=adminClient.getStoreClientConfigString(Arrays.asList(STORE_NAME),ADMIN_URL);
  assertTrue(ClientConfigUtil.compareMultipleClientConfigAvro(putConfigAvro,getConfigAvro1));
}
