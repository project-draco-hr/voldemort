{
  this.selector=selector;
  this.socketChannel=socketChannel;
  this.requestHandler=requestHandler;
  inputStream=new DataInputStream(new BufferedInputStream(new ChannelBackedInputStream(socketChannel),BUFFER_SIZE));
  outputStream=new DataOutputStream(new BufferedOutputStream(new ChannelBackedOutputStream(socketChannel),BUFFER_SIZE));
  if (logger.isInfoEnabled())   logger.info("Accepting remote connection from " + socketChannel.socket().getRemoteSocketAddress());
}
