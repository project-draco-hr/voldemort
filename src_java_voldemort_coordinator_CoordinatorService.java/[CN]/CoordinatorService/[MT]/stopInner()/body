{
  if (this.nettyServerChannel != null) {
    this.nettyServerChannel.close();
  }
  JmxUtils.unregisterMbean(JmxUtils.createObjectName(JmxUtils.getPackageName(this.getClass()),JmxUtils.getClassName(this.getClass())));
  JmxUtils.unregisterMbean(JmxUtils.createObjectName(JmxUtils.getPackageName(this.errorStats.getClass()),JmxUtils.getClassName(this.errorStats.getClass())));
}
