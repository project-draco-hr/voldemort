{
  StringBuilder errors=new StringBuilder();
  try {
    run("SshClusterStarter.ssh",hostNames,hostUserId,sshPrivateKey,voldemortRootDirectory,voldemortHomeDirectory,null,timeout,errors);
  }
 catch (  IOException e) {
    throw new StartClusterException(e);
  }
  if (errors.length() > 0)   throw new StartClusterException(errors.toString());
}
