{
  StringBuilder sb=new StringBuilder();
  sb.append("Current Cluster: ").append(Utils.NEWLINE).append(RebalanceUtils.printMap(currentAllStoresNodeIdToAllPartitionTuples)).append(Utils.NEWLINE);
  sb.append("Target Cluster: ").append(Utils.NEWLINE).append(RebalanceUtils.printMap(targetAllStoresNodeIdToAllPartitionTuples));
  return sb.toString();
}
