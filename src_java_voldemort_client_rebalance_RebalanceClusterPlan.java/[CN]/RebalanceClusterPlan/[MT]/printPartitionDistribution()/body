{
  StringBuilder sb=new StringBuilder();
  sb.append("Current Cluster: ").append(Utils.NEWLINE).append(RebalanceUtils.printMap(nodeIdToAllPartitions,currentCluster)).append(Utils.NEWLINE);
  sb.append("Target Cluster: ").append(Utils.NEWLINE).append(RebalanceUtils.printMap(targetNodeIdToAllPartitions,targetCluster));
  return sb.toString();
}
