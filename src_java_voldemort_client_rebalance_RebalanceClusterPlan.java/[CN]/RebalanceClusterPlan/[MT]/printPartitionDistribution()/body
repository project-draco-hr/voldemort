{
  StringBuilder sb=new StringBuilder();
  sb.append("Current Cluster: ").append(Utils.NEWLINE).append(RebalanceUtils.printMap(currentNodeIdToAllPartitionTuples,currentCluster)).append(Utils.NEWLINE);
  sb.append("Target Cluster: ").append(Utils.NEWLINE).append(RebalanceUtils.printMap(targetNodeIdToAllPartitionTuples,targetCluster));
  return sb.toString();
}
