{
switch (value) {
case 0:
    return GET_METADATA;
case 1:
  return UPDATE_METADATA;
case 2:
return UPDATE_PARTITION_ENTRIES;
case 3:
return FETCH_PARTITION_ENTRIES;
case 4:
return DELETE_PARTITION_ENTRIES;
case 5:
return INITIATE_FETCH_AND_UPDATE;
case 6:
return ASYNC_OPERATION_STATUS;
case 7:
return TRUNCATE_ENTRIES;
default :
return null;
}
}
