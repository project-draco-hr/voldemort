{
  if (result.hasAsyncStatus() && result.asyncStatus_ != voldemort.client.protocol.pb.VAdminProto.AsyncStatusRequest.getDefaultInstance()) {
    result.asyncStatus_=voldemort.client.protocol.pb.VAdminProto.AsyncStatusRequest.newBuilder(result.asyncStatus_).mergeFrom(value).buildPartial();
  }
 else {
    result.asyncStatus_=value;
  }
  result.hasAsyncStatus=true;
  return this;
}
