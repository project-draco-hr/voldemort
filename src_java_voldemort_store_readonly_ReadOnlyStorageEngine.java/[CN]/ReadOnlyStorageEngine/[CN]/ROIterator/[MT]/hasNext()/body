{
  if (chunksFinished)   return false;
  while (currentChunk != chunkedFileSet.getNumChunks() - 1 && chunkedFileSet.getDataFileSize(currentChunk) == 0) {
    currentChunk++;
  }
  if (currentOffsetInChunk >= chunkedFileSet.getDataFileSize(currentChunk) && currentChunk == chunkedFileSet.getNumChunks() - 1) {
    return false;
  }
  return true;
}
