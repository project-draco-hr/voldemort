{
  File latestDir=ReadOnlyUtils.getLatestDir(storeDir);
  if (latestDir != null)   return latestDir;
  File[] versionDirs=ReadOnlyUtils.getVersionDirs(storeDir);
  if (versionDirs == null || versionDirs.length == 0) {
    return null;
  }
 else {
    return ReadOnlyUtils.findKthVersionedDir(versionDirs,versionDirs.length - 1,versionDirs.length - 1)[0];
  }
}
