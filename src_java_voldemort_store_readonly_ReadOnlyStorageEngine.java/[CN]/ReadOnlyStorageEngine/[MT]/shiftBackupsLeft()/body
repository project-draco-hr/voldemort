{
  if (isOpen)   throw new VoldemortException("Can't move backup files while store is open.");
  File primary=new File(storeDir,"version-0");
  DateFormat df=new SimpleDateFormat("MM-dd-yyyy");
  if (primary.exists())   Utils.move(primary,new File(storeDir,"version-0." + df.format(new Date()) + ".bak"));
  shiftBackupsLeft(0);
}
