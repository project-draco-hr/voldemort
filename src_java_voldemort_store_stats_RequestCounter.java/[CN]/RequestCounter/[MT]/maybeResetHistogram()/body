{
  if (!this.useHistogram)   return;
  Accumulator accum=values.get();
  long now=time.getMilliseconds();
  if (now - accum.startTimeMS > durationMs) {
    long startTimeNs=System.nanoTime();
    q95LatencyMs=histogram.getQuantile(0.95);
    q99LatencyMs=histogram.getQuantile(0.99);
    histogram.reset();
    logger.info("Histogram (" + System.identityHashCode(histogram) + ") : reset, Q95, & Q99 took "+ (System.nanoTime() - startTimeNs)+ " ns.");
  }
}
