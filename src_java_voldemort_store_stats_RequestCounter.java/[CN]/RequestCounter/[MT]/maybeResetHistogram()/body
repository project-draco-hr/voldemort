{
  if (!this.useHistogram)   return;
  Accumulator accum=values.get();
  long now=time.getMilliseconds();
  if (now - accum.startTimeMS > durationMs) {
    long startTimeNs=0;
    if (logger.isDebugEnabled()) {
      startTimeNs=System.nanoTime();
    }
    histogramHundredUs.reset();
    histogramSecond.reset();
    if (logger.isDebugEnabled()) {
      logger.debug("Histogram (" + System.identityHashCode(histogramHundredUs) + ") : reset, Q95, & Q99 took "+ (System.nanoTime() - startTimeNs)+ " ns.");
    }
  }
}
