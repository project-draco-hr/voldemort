{
  String avroConfig=new String();
  Boolean firstProp=true;
  for (  String key : props.stringPropertyNames()) {
    if (firstProp) {
      firstProp=false;
    }
 else {
      avroConfig=avroConfig + ", ";
    }
    avroConfig=avroConfig + "\"" + key+ "\": \""+ props.getProperty(key)+ "\"";
  }
  return "{" + avroConfig + "}";
}
