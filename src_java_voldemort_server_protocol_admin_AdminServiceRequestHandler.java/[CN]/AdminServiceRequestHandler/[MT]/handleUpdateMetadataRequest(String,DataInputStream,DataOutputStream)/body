{
  if (keyString.equals(MetadataStore.CLUSTER_KEY) || keyString.equals(MetadataStore.ROLLBACK_CLUSTER_KEY)) {
    handleUpdateClusterMetadataRequest(keyString,inputStream,outputStream);
  }
 else   if (keyString.equals(MetadataStore.STORES_KEY)) {
    handleUpdateStoresMetadataRequest(keyString,inputStream,outputStream);
  }
 else {
    writeException(outputStream,new VoldemortException("Metadata Key passed " + keyString + " is not handled yet ..."));
  }
}
