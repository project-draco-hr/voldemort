{
  boolean fetchValues=request.hasFetchValues() && request.getFetchValues();
  if (fetchValues) {
    boolean fetchMasterValues=request.hasFetchMasterEntries() && request.getFetchMasterEntries();
    if (fetchMasterValues) {
      return new FetchMasterEntriesStreamRequestHandler(request,metadataStore,errorCodeMapper,voldemortConfig,storeRepository,networkClassLoader);
    }
 else {
      return new FetchEntriesStreamRequestHandler(request,metadataStore,errorCodeMapper,voldemortConfig,storeRepository,networkClassLoader);
    }
  }
 else   return new FetchKeysStreamRequestHandler(request,metadataStore,errorCodeMapper,voldemortConfig,storeRepository,networkClassLoader);
}
