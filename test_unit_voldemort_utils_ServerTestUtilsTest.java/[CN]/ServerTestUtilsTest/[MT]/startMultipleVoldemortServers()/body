{
  Cluster cluster=ServerTestUtils.getLocalCluster(8,new int[][]{{0},{1},{2},{3},{4},{5},{6},{7}});
  VoldemortServer[] servers=new VoldemortServer[8];
  for (int i=0; i < 8; i++) {
    servers[i]=ServerTestUtils.startVoldemortServerInMannerThatMayResultInBindException(socketStoreFactory,ServerTestUtils.createServerConfig(true,i,TestUtils.createTempDir().getAbsolutePath(),null,storesXmlfile,new Properties()),cluster);
  }
  assertTrue(true);
}
