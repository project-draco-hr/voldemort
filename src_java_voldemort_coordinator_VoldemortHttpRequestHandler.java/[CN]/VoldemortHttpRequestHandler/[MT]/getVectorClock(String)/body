{
  VectorClock vc=null;
  ObjectMapper mapper=new ObjectMapper();
  if (logger.isDebugEnabled()) {
    logger.debug("Received vector clock : " + vectorClockHeader);
  }
  try {
    VectorClockWrapper vcWrapper=mapper.readValue(vectorClockHeader,VectorClockWrapper.class);
    vc=new VectorClock(vcWrapper.getVersions(),vcWrapper.getTimestamp());
  }
 catch (  NullPointerException npe) {
  }
catch (  JsonParseException e) {
    e.printStackTrace();
  }
catch (  JsonMappingException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  return vc;
}
