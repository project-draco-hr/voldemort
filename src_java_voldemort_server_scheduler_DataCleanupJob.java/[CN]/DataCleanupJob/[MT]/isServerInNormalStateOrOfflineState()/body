{
  if (metadataStore != null && metadataStore.getServerStateUnlocked() != VoldemortState.NORMAL_SERVER && metadataStore.getServerStateUnlocked() != VoldemortState.OFFLINE_SERVER) {
    logger.info("Datacleanup on store " + store.getName() + " skipped since server is not normal nor offline..");
    return false;
  }
  return true;
}
