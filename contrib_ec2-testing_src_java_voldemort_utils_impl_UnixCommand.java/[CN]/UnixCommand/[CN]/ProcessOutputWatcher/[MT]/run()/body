{
  try {
    String line=null;
    while ((line=reader.readLine()) != null) {
      try {
        if (logger.isDebugEnabled())         logger.debug(line);
        commandOutputListener.outputReceived(hostName,line);
      }
 catch (      Throwable t) {
        t.printStackTrace();
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
