{
  try {
    String line=null;
    while ((line=reader.readLine()) != null) {
      try {
        if (logger.isTraceEnabled())         logger.trace(line);
        commandOutputListener.outputReceived(hostName,line);
      }
 catch (      Throwable t) {
        t.printStackTrace();
      }
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
