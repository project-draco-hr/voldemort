{
  closeStreamingSessions();
  Future future=streamingresults.submit(resetCheckpointCallback);
  try {
    future.get();
  }
 catch (  InterruptedException e1) {
    logger.warn("Reset check point interrupted" + e1);
  }
catch (  ExecutionException e1) {
    logger.warn("Reset check point interrupted" + e1);
  }
}
