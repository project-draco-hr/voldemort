{
  this.parent=null;
  this.statsMap=new ConcurrentHashMap<SocketDestination,ClientSocketStats>();
  this.destination=null;
  this.pool=null;
  this.jmxId=jmxId;
  logger.info("Constructed ClientSocketStatsStats object (" + System.identityHashCode(this) + ") with parent object("+ System.identityHashCode(parent)+ ")");
}
