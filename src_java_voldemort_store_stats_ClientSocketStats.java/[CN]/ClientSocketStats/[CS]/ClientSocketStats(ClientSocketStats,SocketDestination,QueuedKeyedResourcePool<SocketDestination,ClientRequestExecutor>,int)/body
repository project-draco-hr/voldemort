{
  this.parent=parent;
  this.statsMap=null;
  this.destination=destination;
  this.pool=pool;
  this.jmxId=jmxId;
  if (logger.isDebugEnabled()) {
    logger.debug("Constructed ClientSocketStatsStats object (" + System.identityHashCode(this) + ") with parent object("+ System.identityHashCode(parent)+ ")");
  }
}
