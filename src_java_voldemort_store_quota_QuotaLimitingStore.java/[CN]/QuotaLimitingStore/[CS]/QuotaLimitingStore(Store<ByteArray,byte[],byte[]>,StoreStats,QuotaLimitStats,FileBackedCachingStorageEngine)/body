{
  super(innerStore);
  this.storeStats=storeStats;
  this.quotaStore=quotaStore;
  this.getQuotaKey=QuotaUtils.makeQuotaKey(innerStore.getName(),QuotaType.GET_THROUGHPUT);
  this.putQuotaKey=QuotaUtils.makeQuotaKey(innerStore.getName(),QuotaType.PUT_THROUGHPUT);
  this.getAllQuotaKey=QuotaUtils.makeQuotaKey(innerStore.getName(),QuotaType.GET_ALL_THROUGHPUT);
  this.deleteQuotaKey=QuotaUtils.makeQuotaKey(innerStore.getName(),QuotaType.DELETE_THROUGHPUT);
  this.quotaStats=quotaStats;
}
