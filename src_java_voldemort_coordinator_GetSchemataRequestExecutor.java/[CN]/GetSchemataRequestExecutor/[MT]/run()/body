{
  StoreDefinition storeDef=StoreDefinitionUtils.getStoreDefinitionWithName(storeClientFactory.getStoreDefs(),storeName);
  String serializerInfoXml=CoordinatorUtils.constructSerializerInfoXml(storeDef);
  try {
    writeResponse(serializerInfoXml.getBytes("UTF-8"));
  }
 catch (  UnsupportedEncodingException e) {
    logger.error(e);
  }
}
