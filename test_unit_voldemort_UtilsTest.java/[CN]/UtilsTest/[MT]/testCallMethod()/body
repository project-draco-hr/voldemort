{
  assertEquals("abcd",Utils.callMethod("ABCD",String.class,"toLowerCase",new Class<?>[0],new Object[0]));
  assertEquals('B',Utils.callMethod("ABCD",String.class,"charAt",new Class<?>[]{int.class},new Object[]{1}));
  try {
    Utils.callMethod("ABCD",String.class,"nonExistantMethod",new Class<?>[0],new Object[0]);
    fail("Called non-existant method");
  }
 catch (  IllegalArgumentException e) {
  }
}
