{
  long queryTime=System.nanoTime() - start;
  NumberFormat nf=NumberFormat.getInstance();
  nf.setMaximumFractionDigits(4);
  System.out.println("Throughput: " + (successes / (float)queryTime * Time.NS_PER_SECOND) + " "+ noun+ "/sec.");
  System.out.println("Avg. " + noun + " latency: "+ nf.format(TestUtils.mean(requestTimes))+ " ms.");
  System.out.println("95th percentile " + noun + " latency: "+ TestUtils.quantile(requestTimes,.95)+ " ms.");
  System.out.println("99th percentile " + noun + " latency: "+ TestUtils.quantile(requestTimes,.99)+ " ms.");
  System.out.println(successes + " successful " + noun+ ".");
}
