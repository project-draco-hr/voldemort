{
  List<String> targets=new ArrayList<String>();
  File file=new File(path);
  BufferedReader reader=null;
  try {
    reader=new BufferedReader(new FileReader(file));
    String text=null;
    while ((text=reader.readLine()) != null) {
      targets.add(text);
    }
  }
  finally {
    try {
      if (reader != null) {
        reader.close();
      }
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  return targets;
}
