{
  assertEquals("Two empty clocks merge to an empty clock.",getClock().merge(getClock()),getClock());
  assertEquals("Merging two clocks should be the same as taking the max of their event counts.",getClock(1).merge(getClock(2)),getClock(1,2));
  assertEquals("Merging two clocks should be the same as taking the max of their event counts.",getClock(1,1,1,2,3).merge(getClock(1,2,2,4)),getClock(1,1,1,2,2,3,4));
}
