{
  Versioned<byte[]> value=doGetRemoteMetadata(nodeId,new ByteArray(ByteUtils.getBytes(MetadataStore.STORES_KEY,"UTF-8")));
  List<StoreDefinition> storeList=storeMapper.readStoreList(new StringReader(ByteUtils.getString(value.getValue(),"UTF-8")));
  return new Versioned<List<StoreDefinition>>(storeList,value.getVersion());
}
