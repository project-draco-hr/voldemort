{
  VectorClock oldClock=(VectorClock)getRebalancingProxyDest(rebalancingNodeId).getVersion();
  doUpdateRemoteMetadata(rebalancingNodeId,new ByteArray(ByteUtils.getBytes(MetadataStore.REBALANCING_PROXY_DEST,"UTF-8")),new Versioned<byte[]>(ByteUtils.getBytes("" + proxyDestNodeId,"UTF-8"),oldClock.incremented(rebalancingNodeId,1)));
}
