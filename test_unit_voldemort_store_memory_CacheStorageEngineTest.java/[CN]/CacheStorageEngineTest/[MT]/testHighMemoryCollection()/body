{
  long maxMemory=Runtime.getRuntime().maxMemory();
  int objectSize=Math.max((int)maxMemory / NUM_OBJECTS,1);
  StorageEngine<ByteArray,byte[]> engine=getStorageEngine();
  for (int i=0; i < NUM_OBJECTS; i++)   engine.put(TestUtils.toByteArray(Integer.toString(i)),new Versioned<byte[]>(TestUtils.randomBytes(objectSize)));
}
