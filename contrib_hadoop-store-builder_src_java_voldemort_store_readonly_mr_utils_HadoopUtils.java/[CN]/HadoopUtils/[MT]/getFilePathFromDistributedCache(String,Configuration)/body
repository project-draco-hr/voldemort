{
  Path[] paths=DistributedCache.getLocalCacheFiles(conf);
  Path filePath=null;
  File file=new File(filename);
  if (paths == null) {
    if (file.exists())     filePath=new Path(file.getAbsolutePath());
  }
 else {
    for (    Path path : paths)     if (path.getName().equals(file.getName()))     filePath=path;
  }
  return filePath;
}
