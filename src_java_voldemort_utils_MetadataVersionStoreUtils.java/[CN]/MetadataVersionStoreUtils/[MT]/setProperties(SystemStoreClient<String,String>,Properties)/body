{
  if (props == null) {
    return;
  }
  try {
    String versionString=getPropertiesString(props);
    versionStore.putSysStore(SystemStoreConstants.VERSIONS_METADATA_KEY,versionString);
  }
 catch (  Exception e) {
    logger.debug("Got exception in setting properties : " + e.getMessage());
  }
}
