{
  for (  final Integer node : nodePartitions.keySet()) {
    System.out.println("Testing fetch of node " + node + " partitions "+ nodePartitions.get(node)+ ": \n");
    measureFunction(new Measurable(){
      public long apply(){
        long i=0;
        Iterator<Pair<ByteArray,Versioned<byte[]>>> result=adminClient.fetchPartitionEntries(node,storeName,new ArrayList<Integer>(nodePartitions.get(node)),null);
        while (result.hasNext()) {
          i++;
          result.next();
        }
        return i;
      }
    }
,1);
  }
}
