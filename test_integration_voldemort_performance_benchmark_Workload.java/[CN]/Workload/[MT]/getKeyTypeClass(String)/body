{
  if (Benchmark.IDENTITY_KEY_TYPE.equals(keyType)) {
    return byte[].class;
  }
 else   if (Benchmark.JSONINT_KEY_TYPE.equals(keyType)) {
    return Integer.class;
  }
 else   if (Benchmark.JSONSTRING_KEY_TYPE.equals(keyType)) {
    return String.class;
  }
 else {
    return String.class;
  }
}
