{
  this.numEntries++;
  this.keyBytes+=keySize;
  this.valueBytes+=valueSize;
  if (keySize > this.maxKeySize)   this.maxKeySize=keySize;
  if (keySize < this.minKeySize)   this.minKeySize=keySize;
  if (valueSize > this.maxValueSize)   this.maxValueSize=valueSize;
  if (valueSize < this.minValueSize)   this.minValueSize=valueSize;
}
