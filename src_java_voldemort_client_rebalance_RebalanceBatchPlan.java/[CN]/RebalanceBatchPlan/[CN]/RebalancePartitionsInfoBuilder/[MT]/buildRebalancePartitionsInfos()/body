{
  final List<RebalancePartitionsInfo> result=new ArrayList<RebalancePartitionsInfo>();
  for (  Pair<Integer,Integer> stealerDonor : stealerDonorToStoreToStealPartition.keySet()) {
    result.add(new RebalancePartitionsInfo(stealerDonor.getFirst(),stealerDonor.getSecond(),stealerDonorToStoreToStealPartition.get(stealerDonor),currentCluster));
  }
  return result;
}
