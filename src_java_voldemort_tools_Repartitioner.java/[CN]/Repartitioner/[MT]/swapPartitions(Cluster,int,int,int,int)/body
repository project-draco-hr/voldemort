{
  Cluster returnCluster=ClusterUtils.copyCluster(nextCluster);
  returnCluster=RebalanceUtils.createUpdatedCluster(returnCluster,nodeIdA,Lists.newArrayList(partitionIdB));
  returnCluster=RebalanceUtils.createUpdatedCluster(returnCluster,nodeIdB,Lists.newArrayList(partitionIdA));
  return returnCluster;
}
