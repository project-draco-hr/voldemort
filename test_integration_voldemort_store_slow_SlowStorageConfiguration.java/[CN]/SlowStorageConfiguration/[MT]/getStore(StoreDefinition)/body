{
  if (voldemortConfig != null) {
    return new SlowStorageEngine<ByteArray,byte[],byte[]>(storeDef.getName(),this.voldemortConfig.testingGetSlowQueueingDelays(),this.voldemortConfig.testingGetSlowConcurrentDelays());
  }
  return new SlowStorageEngine<ByteArray,byte[],byte[]>(storeDef.getName(),new OpTimeMap(0),new OpTimeMap(0));
}
