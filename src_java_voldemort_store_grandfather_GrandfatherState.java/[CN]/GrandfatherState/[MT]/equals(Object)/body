{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  GrandfatherState that=(GrandfatherState)o;
  if (!this.stealInfoMap.equals(that.stealInfoMap))   return false;
  if (!this.storeDefs.equals(that.storeDefs))   return false;
  if (!this.cluster.equals(that.cluster))   return false;
  return true;
}
