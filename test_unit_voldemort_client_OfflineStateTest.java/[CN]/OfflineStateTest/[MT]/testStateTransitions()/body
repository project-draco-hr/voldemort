{
  AdminClient client=getAdminClient();
  assertTrue(testOnlineTraffic());
  assertTrue(testSlopStreaming());
  toOfflineState(client);
  assertFalse(testOnlineTraffic());
  assertFalse(testSlopStreaming());
  toNormalState(client);
  assertTrue(testOnlineTraffic());
  assertTrue(testSlopStreaming());
  toOfflineState(client);
  assertFalse(testOnlineTraffic());
  assertFalse(testSlopStreaming());
  toNormalState(client);
  assertTrue(testOnlineTraffic());
  assertTrue(testSlopStreaming());
  toOfflineState(client);
  assertFalse(testOnlineTraffic());
  assertFalse(testSlopStreaming());
  toNormalState(client);
  assertTrue(testOnlineTraffic());
  assertTrue(testSlopStreaming());
}
