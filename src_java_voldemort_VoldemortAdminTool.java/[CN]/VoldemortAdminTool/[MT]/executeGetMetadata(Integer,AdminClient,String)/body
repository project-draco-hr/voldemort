{
  Versioned<String> versioned=adminClient.getRemoteMetadata(nodeId,metadataKey);
  if (versioned == null) {
    System.out.println("null");
  }
 else {
    System.out.println(versioned.getVersion());
    System.out.print(": ");
    System.out.println(versioned.getValue());
    System.out.println();
  }
}
