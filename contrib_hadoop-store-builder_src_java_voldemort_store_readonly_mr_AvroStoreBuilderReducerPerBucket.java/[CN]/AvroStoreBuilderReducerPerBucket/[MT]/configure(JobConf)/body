{
  JobConf conf=job;
  try {
    keyValueWriterClass=conf.getClass("writer.class",null);
    if (keyValueWriterClass != null)     writer=(KeyValueWriter)ReflectUtils.callConstructor(keyValueWriterClass);
 else     writer=new HadoopStoreWriterPerBucket();
    writer.conf(job);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
