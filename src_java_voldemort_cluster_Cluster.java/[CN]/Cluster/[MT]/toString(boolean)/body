{
  if (!isDetailed) {
    return toString();
  }
  StringBuilder builder=new StringBuilder(String.format("Cluster [%s] Nodes [%d] Zones [%d] Partitions [%d]\n",getName(),getNumberOfNodes(),getNumberOfZones(),getNumberOfPartitions()));
  for (  Zone zone : getZones()) {
    builder.append(zone.toString());
    builder.append("\n");
  }
  for (  Node node : getNodes()) {
    builder.append(node.toString());
    builder.append("\n");
  }
  return builder.toString();
}
