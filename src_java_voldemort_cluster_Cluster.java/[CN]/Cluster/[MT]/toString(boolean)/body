{
  if (!isDetailed) {
    return toString();
  }
  StringBuilder builder=new StringBuilder("Cluster [" + getName() + "] Nodes ["+ getNumberOfNodes()+ "] Zones ["+ getNumberOfZones()+ "] Partitions ["+ getNumberOfPartitions()+ "]");
  builder.append(" Zone Info [" + getZones() + "]");
  builder.append(" Node Info [" + getNodes() + "]");
  return builder.toString();
}
