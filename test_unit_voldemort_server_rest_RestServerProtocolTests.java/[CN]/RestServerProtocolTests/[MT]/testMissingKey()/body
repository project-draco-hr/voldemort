{
  logger.info("********** Testing missing key **********");
  HttpURLConnection conn=null;
  int responseCode=-1;
  try {
    conn=doGet(urlStr,null,storeNameStr,String.valueOf(originTime),String.valueOf(timeOut),String.valueOf(routingType));
    responseCode=conn.getResponseCode();
    logger.info("Response Code: " + responseCode + " Message: "+ conn.getResponseMessage());
    readErrorMessageFromResponse(conn);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  assertEquals(responseCode,400);
  conn.disconnect();
}
