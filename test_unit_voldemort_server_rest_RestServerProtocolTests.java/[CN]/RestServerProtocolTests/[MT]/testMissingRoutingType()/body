{
  logger.info("********** Testing missing routing type **********");
  HttpURLConnection conn=null;
  int responseCode=-1;
  try {
    conn=doPut(urlStr,key1,value1,storeNameStr,String.valueOf(originTime),String.valueOf(timeOut),null,eTag,contentType,String.valueOf(value1.getBytes().length));
    responseCode=conn.getResponseCode();
    logger.info("Response Code: " + responseCode + " Message: "+ conn.getResponseMessage());
    readErrorMessageFromResponse(conn);
  }
 catch (  IOException e1) {
    e1.printStackTrace();
  }
  assertEquals(responseCode,400);
  conn.disconnect();
}
