{
  ClientConfig config=new ClientConfig().setMaxConnectionsPerNode(1).setMaxThreads(1).setConnectionTimeout(voldemortConfig.getAdminConnectionTimeout(),TimeUnit.MILLISECONDS).setSocketTimeout(voldemortConfig.getSocketTimeoutMs(),TimeUnit.MILLISECONDS).setSocketBufferSize(voldemortConfig.getAdminSocketBufferSize());
  return new ProtoBuffAdminClientRequestFormat(cluster,config);
}
