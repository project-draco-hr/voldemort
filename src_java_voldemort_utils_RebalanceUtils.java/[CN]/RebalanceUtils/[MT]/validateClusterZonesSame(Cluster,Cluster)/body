{
  Set<Zone> lhsSet=new HashSet<Zone>(lhs.getZones());
  Set<Zone> rhsSet=new HashSet<Zone>(rhs.getZones());
  if (!lhsSet.equals(rhsSet))   throw new VoldemortException("Zones are not the same [ lhs cluster zones (" + lhs.getZones() + ") not equal to rhs cluster zones ("+ rhs.getZones()+ ") ]");
}
