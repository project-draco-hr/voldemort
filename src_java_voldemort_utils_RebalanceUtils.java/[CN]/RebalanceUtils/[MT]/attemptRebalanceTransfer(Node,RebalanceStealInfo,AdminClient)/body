{
  try {
    int rebalanceAsyncId=adminClient.rebalanceNode(stealerNode.getId(),stealInfo);
    adminClient.waitForCompletion(stealerNode.getId(),rebalanceAsyncId,24 * 60 * 60,TimeUnit.SECONDS);
    return true;
  }
 catch (  Exception e) {
    logger.warn("Failed Attempt number " + stealInfo.getAttempt() + " Rebalance transfer "+ stealerNode.getId()+ " <== "+ stealInfo.getDonorId()+ " "+ stealInfo.getPartitionList()+ " with exception:",e);
  }
  return false;
}
