{
  if (fs1.isDir())   return fs2.isDir() ? 0 : -1;
 else   if (fs1.getPath().getName().endsWith("checkSum.txt"))   return -1;
 else   if (fs2.getPath().getName().endsWith("checkSum.txt"))   return 1;
 else   if (fs1.getPath().getName().endsWith(".index"))   return fs2.getPath().getName().endsWith(".index") ? 0 : 1;
 else   return 0;
}
