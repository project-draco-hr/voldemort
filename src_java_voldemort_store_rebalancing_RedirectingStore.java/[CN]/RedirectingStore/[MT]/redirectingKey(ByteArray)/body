{
  if (VoldemortState.REBALANCING_MASTER_SERVER.equals(metadata.getServerState()) && isRedirectingStoreEnabled.get()) {
    return metadata.getRebalancerState().find(getName(),metadata.getRoutingStrategy(getName()).getPartitionList(key.get()),metadata.getCluster().getNodeById(metadata.getNodeId()).getPartitionIds());
  }
  return null;
}
