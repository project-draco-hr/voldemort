{
  if (!storeRepository.hasRedirectingSocketStore(getName(),metadata.getRebalancingStealInfo().getDonorId())) {
    throw new VoldemortException("RedirectingSocketStore not present in storeRepository for (store,nodeId) pair (" + getName() + ","+ metadata.getRebalancingStealInfo().getDonorId()+ ").");
  }
  return storeRepository.getRedirectingSocketStore(getName(),metadata.getRebalancingStealInfo().getDonorId()).getAll(keys);
}
