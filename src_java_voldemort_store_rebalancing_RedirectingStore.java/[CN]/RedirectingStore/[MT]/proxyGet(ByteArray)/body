{
  if (!storeRepository.hasNodeStore(getName(),metadata.getRebalancingDonorNodeId())) {
    throw new VoldemortException("Node Store not present in storeRepository for (store,nodeId) pair (" + getName() + ","+ metadata.getRebalancingDonorNodeId()+ ").");
  }
  return storeRepository.getNodeStore(getName(),metadata.getRebalancingDonorNodeId()).get(key);
}
