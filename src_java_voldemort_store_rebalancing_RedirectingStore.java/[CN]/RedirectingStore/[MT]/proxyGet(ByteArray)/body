{
  if (!storeRepository.hasNodeStore(getName(),metadata.getRebalancingSlaveNodeId())) {
    throw new VoldemortException("Node Store not present in storeRepository for (store,nodeId) pair (" + getName() + ","+ metadata.getRebalancingSlaveNodeId()+ ").");
  }
  return storeRepository.getNodeStore(getName(),metadata.getRebalancingSlaveNodeId()).get(key);
}
