{
  if (!storeRepository.hasNodeStore(getName(),metadata.getRebalancingStealInfo().getDonorId())) {
    throw new VoldemortException("Node Store not present in storeRepository for (store,nodeId) pair (" + getName() + ","+ metadata.getRebalancingStealInfo().getDonorId()+ ").");
  }
  return storeRepository.getNodeStore(getName(),metadata.getRebalancingStealInfo().getDonorId()).get(key);
}
