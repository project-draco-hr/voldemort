{
  if (failureDetector != null)   failureDetector.destroy();
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(failureDetectorClass.getName()).setNodeBannagePeriod(1000).setNodes(cluster.getNodes()).setStoreResolver(createMutableStoreResolver(subStores));
  failureDetector=create(failureDetectorConfig);
}
