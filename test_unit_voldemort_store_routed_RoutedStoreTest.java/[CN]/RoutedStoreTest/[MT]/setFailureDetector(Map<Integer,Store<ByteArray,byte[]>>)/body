{
  if (failureDetector != null)   failureDetector.destroy();
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(failureDetectorClass.getName()).setBannagePeriod(1000).setNodes(cluster.getNodes()).setStoreVerifier(create(subStores));
  failureDetector=create(failureDetectorConfig);
}
