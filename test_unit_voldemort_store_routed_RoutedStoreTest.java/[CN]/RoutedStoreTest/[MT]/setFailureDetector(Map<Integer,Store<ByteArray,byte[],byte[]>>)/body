{
  if (failureDetector != null)   failureDetector.destroy();
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(failureDetectorClass.getName()).setBannagePeriod(BANNAGE_PERIOD).setCluster(cluster).setStoreVerifier(create(subStores));
  failureDetector=create(failureDetectorConfig,false);
}
