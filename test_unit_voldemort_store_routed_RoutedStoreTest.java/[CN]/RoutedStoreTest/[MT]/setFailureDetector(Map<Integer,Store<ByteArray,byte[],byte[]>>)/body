{
  if (failureDetector != null)   failureDetector.destroy();
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(failureDetectorClass.getName()).setBannagePeriod(BANNAGE_PERIOD).setNodes(cluster.getNodes()).setStoreVerifier(create(subStores));
  failureDetector=create(failureDetectorConfig,false);
}
