{
  writer.write('{');
  Set<Map.Entry<String,Object>> entrySet=values.entrySet();
  int index=0;
  for (  Map.Entry<String,Object> entry : entrySet) {
    writer.write("'");
    writer.write(entry.getKey());
    writer.write("'");
    writer.write(':');
    write(entry.getValue());
    if (++index < entrySet.size()) {
      writer.write(", ");
    }
  }
  writer.write('}');
}
