{
  Store<ByteArray,byte[]> store=null;
  if (node.getId() == voldemortConfig.getNodeId()) {
    store=metadataStore;
  }
 else {
synchronized (stores) {
      store=stores.get(node.getId());
      if (store == null) {
        store=new SocketStore(MetadataStore.METADATA_STORE_NAME,new SocketDestination(node.getHost(),node.getSocketPort(),voldemortConfig.getRequestFormatType()),socketPool,false);
        stores.put(node.getId(),store);
      }
    }
  }
  store.get(KEY);
}
