{
  StoreUtils.assertValidKey(key);
  Cursor cursor=null;
  try {
    cursor=bdbDatabase.openCursor(null,null);
    return get(cursor,key,lockMode,serializer);
  }
 catch (  DatabaseException e) {
    logger.error(e);
    throw new PersistenceFailureException(e);
  }
 finally {
    attemptClose(cursor);
  }
}
