{
  if (logger.isTraceEnabled())   logger.trace(node + " set as available");
  System.out.println(node + " set as available");
  NodeStatus nodeStatus=getNodeStatus(node);
synchronized (nodeStatus) {
    boolean previouslyAvailable=nodeStatus.isAvailable();
    nodeStatus.setAvailable(true);
    if (!previouslyAvailable)     notifyAvailable(node);
  }
}
