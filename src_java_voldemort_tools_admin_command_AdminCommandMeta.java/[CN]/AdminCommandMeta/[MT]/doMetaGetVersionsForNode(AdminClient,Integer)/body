{
  ByteArray keyArray=new ByteArray(MetadataVersionStoreUtils.VERSIONS_METADATA_KEY.getBytes("UTF8"));
  List<Versioned<byte[]>> valueObj=adminClient.storeOps.getNodeKey(SystemStoreConstants.SystemStoreName.voldsys$_metadata_version_persistence.name(),nodeId,keyArray);
  if (valueObj.size() != 1) {
    throw new IOException("Expected one value for the key " + MetadataVersionStoreUtils.VERSIONS_METADATA_KEY + " on node id "+ nodeId+ " but found "+ valueObj.size());
  }
  Properties props=new Properties();
  props.load(new ByteArrayInputStream(valueObj.get(0).getValue()));
  return props;
}
