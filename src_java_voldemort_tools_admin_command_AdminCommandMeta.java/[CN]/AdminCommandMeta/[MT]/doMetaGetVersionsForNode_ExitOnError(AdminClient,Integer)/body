{
  Properties props=null;
  try {
    props=doMetaGetVersionsForNode(adminClient,nodeId);
  }
 catch (  IOException e) {
    System.err.println("Error while retrieving Metadata versions from node : " + nodeId + ". Exception = "+ e.getMessage()+ " \n");
    e.printStackTrace();
    System.exit(-1);
  }
  return props;
}
