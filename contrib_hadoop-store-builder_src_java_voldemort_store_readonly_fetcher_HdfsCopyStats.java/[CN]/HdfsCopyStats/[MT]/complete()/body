{
  long nowMS=System.currentTimeMillis();
  reportStats(" Completed at " + nowMS + "MS. Total bytes Copied "+ totalBytesCopied+ " . Expected Total bytes "+ pathInfo.getTotalSize()+ " . Time taken(MS) "+ (nowMS - startTimeMS));
  if (statsFileWriter != null) {
    IOUtils.closeQuietly(statsFileWriter);
  }
}
