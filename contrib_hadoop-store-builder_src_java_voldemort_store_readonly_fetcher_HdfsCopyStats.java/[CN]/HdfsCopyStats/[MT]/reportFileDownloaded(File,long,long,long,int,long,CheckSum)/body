{
  String fileCheckSum;
  if (checkSum != null) {
    fileCheckSum=ByteUtils.toHexString(checkSum.getCheckSum());
  }
 else {
    fileCheckSum="NO_CHECKSUM";
  }
  reportStats(file.getName() + "," + startTimeMS+ ","+ fileSize+ ","+ timeTakenMS+ ","+ attempts+ ","+ totalBytesTransferred+ ","+ totalBytesWritten+ ","+ fileCheckSum);
}
