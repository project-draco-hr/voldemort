{
  try {
    String quotaKey=QuotaUtils.makeQuotaKey(storeName,quotaType);
    ByteArray keyArray=new ByteArray(quotaKey.getBytes("UTF8"));
    return storeOps.deleteNodeKeyValue(SystemStoreConstants.SystemStoreName.voldsys$_store_quotas.name(),nodeId,keyArray);
  }
 catch (  UnsupportedEncodingException e) {
    throw new VoldemortException(e);
  }
}
