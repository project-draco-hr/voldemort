{
  if (AdminClient.this.cachedBootstrapURLs != null && AdminClient.this.cachedZoneID >= 0) {
    try {
      sysStoreVersion=new SystemStore<String,String>(SystemStoreConstants.SystemStoreName.voldsys$_metadata_version_persistence.name(),AdminClient.this.cachedBootstrapURLs,AdminClient.this.cachedZoneID);
    }
 catch (    Exception e) {
      logger.debug("Error while creating a system store client for metadata version store.");
    }
  }
}
