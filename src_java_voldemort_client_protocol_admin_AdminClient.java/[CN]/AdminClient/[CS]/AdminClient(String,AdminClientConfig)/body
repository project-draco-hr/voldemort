{
  this.currentCluster=getClusterFromBootstrapURL(bootstrapURL);
  this.errorMapper=new ErrorCodeMapper();
  this.pool=createSocketPool(adminClientConfig);
  this.networkClassLoader=new NetworkClassLoader(Thread.currentThread().getContextClassLoader());
  this.adminClientConfig=adminClientConfig;
  cacheSystemStoreParams(bootstrapURL,DEFAULT_ZONE_ID);
}
