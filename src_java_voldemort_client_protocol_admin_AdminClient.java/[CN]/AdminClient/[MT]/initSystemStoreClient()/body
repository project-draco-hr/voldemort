{
  if (this.cachedBootstrapURLs != null && this.cachedZoneID >= 0) {
    try {
      this.sysStoreVersion=new SystemStore<String,String>(SystemStoreConstants.SystemStoreName.voldsys$_metadata_version_persistence.name(),this.cachedBootstrapURLs,this.cachedZoneID);
    }
 catch (    Exception e) {
      logger.debug("Error while creating a system store client for metadata version store.");
    }
  }
}
