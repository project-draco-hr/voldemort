{
  String[] bootstrapUrls=new String[1];
  bootstrapUrls[0]=bootstrapURL;
  try {
    this.sysStoreVersion=new SystemStore<String,String>(SystemStoreConstants.SystemStoreName.voldsys$_metadata_version_persistence.name(),bootstrapUrls,zoneID);
  }
 catch (  Exception e) {
