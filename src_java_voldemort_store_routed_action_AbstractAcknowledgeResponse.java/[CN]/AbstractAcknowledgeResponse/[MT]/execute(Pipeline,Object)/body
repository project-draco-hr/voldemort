{
  Response<K,V> response=(Response<K,V>)eventData;
  pipelineData.incrementCompleted();
  if (logger.isTraceEnabled())   logger.trace("Response received from " + response.getNode().getId() + " for "+ pipeline.getOperation().getSimpleName()+ " - attempts: "+ pipelineData.getAttempts()+ ", completed: "+ pipelineData.getCompleted());
  executeInternal(pipeline,response);
}
