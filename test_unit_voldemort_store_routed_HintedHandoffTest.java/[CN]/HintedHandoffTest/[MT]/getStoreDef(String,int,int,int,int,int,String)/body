{
  SerializerDefinition serDef=new SerializerDefinition("string");
  return new StoreDefinitionBuilder().setName(storeName).setType(InMemoryStorageConfiguration.TYPE_NAME).setKeySerializer(serDef).setValueSerializer(serDef).setRoutingPolicy(RoutingTier.CLIENT).setRoutingStrategyType(strategyType).setReplicationFactor(replicationFactor).setPreferredReads(preads).setRequiredReads(rreads).setPreferredWrites(pwrites).setRequiredWrites(rwrites).setHintedHandoffStrategy(hintRoutingStrategy).build();
}
