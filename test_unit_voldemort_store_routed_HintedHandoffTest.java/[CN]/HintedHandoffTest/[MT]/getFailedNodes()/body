{
  Set<Integer> failedNodes=new CopyOnWriteArraySet<Integer>();
  Random rand=new Random();
  for (int i=0; i < NUM_NODES_FAILED; i++) {
    int n=rand.nextInt(NUM_NODES_TOTAL);
    failedNodes.add(n);
  }
  for (  int node : failedNodes)   getForceFailStore(node).setFail(true);
  if (logger.isTraceEnabled())   logger.trace("Failing requests to " + failedNodes);
  return failedNodes;
}
