{
  if (ServerState.REBALANCING_DONOR_STATE.equals(this.getServerState()) || ServerState.REBALANCING_STEALER_STATE.equals(this.getServerState())) {
    if (newState.equals(ServerState.REBALANCING_DONOR_STATE) || newState.equals(ServerState.REBALANCING_STEALER_STATE))     throw new VoldemortException("Cannot Change State from " + this.getServerState() + " to "+ newState);
  }
  this.serverState=newState;
}
