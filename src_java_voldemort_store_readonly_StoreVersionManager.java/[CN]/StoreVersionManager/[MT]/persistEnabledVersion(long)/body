{
  File disabledMarker=getDisabledMarkerFile(version);
  if (disabledMarker.exists()) {
    if (!disabledMarker.delete()) {
      throw new PersistenceFailureException("Failed to delete the disabled marker for version " + version + " in rootDir: "+ rootDir+ "\nThe store/version will remain enabled only until the next restart.");
    }
  }
}
