{
  boolean result=false;
  String timeoutValStr=this.request.getHeader(RestMessageHeaders.X_VOLD_REQUEST_TIMEOUT_MS);
  if (timeoutValStr != null) {
    try {
      this.parsedRoutingTimeoutInMs=Long.parseLong(timeoutValStr);
      result=true;
    }
 catch (    NumberFormatException nfe) {
      logger.error("Exception when validating request. Incorrect timeout parameter. Cannot parse this to long: " + timeoutValStr,nfe);
      RestServerErrorHandler.writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Incorrect timeout parameter. Cannot parse this to long: " + timeoutValStr);
    }
  }
 else {
    logger.error("Error when validating request. Missing timeout parameter.");
    RestServerErrorHandler.writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Missing timeout parameter.");
  }
  return result;
}
