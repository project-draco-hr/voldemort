{
  StoreUtils.assertValidKey(key);
  ProtoUtils.writeMessage(output,VProto.VoldemortRequest.newBuilder().setType(RequestType.GET).setStore(storeName).setShouldRoute(routingType.equals(RequestRoutingType.ROUTED)).setRequestRouteType(routingType.getRoutingTypeCode()).setGet(VProto.GetRequest.newBuilder().setKey(ByteString.copyFrom(key.get()))).build());
}
