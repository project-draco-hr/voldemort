{
  StoreUtils.assertValidKey(key);
  ProtoUtils.writeMessage(output,VProto.VoldemortRequest.newBuilder().setType(RequestType.DELETE).setStore(storeName).setShouldRoute(routingType.equals(RequestRoutingType.ROUTED)).setIgnoreChecks(routingType.equals(RequestRoutingType.IGNORE_CHECKS)).setDelete(VProto.DeleteRequest.newBuilder().setKey(ByteString.copyFrom(key.get())).setVersion(ProtoUtils.encodeClock(version))).build());
}
