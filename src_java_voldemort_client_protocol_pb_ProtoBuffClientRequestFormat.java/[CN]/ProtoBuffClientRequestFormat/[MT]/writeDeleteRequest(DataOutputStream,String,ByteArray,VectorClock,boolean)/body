{
  StoreUtils.assertValidKey(key);
  ProtoUtils.writeWithSize(output,VProto.VoldemortRequest.newBuilder().setType(RequestType.DELETE).setStore(storeName).setShouldRoute(shouldReroute).setDelete(VProto.DeleteRequest.newBuilder().setKey(ByteString.copyFrom(key.get())).setVersion(ProtoUtils.encodeClock(version))).build());
}
