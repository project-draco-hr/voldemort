{
  List<RebalancePartitionsInfoLiveCycle> stealInfoList=Lists.newArrayListWithExpectedSize(stealInfoMap.size());
  for (  RebalancePartitionsInfoLiveCycle info : getAll())   if (info.getRebalancePartitionsInfo().getUnbalancedStoreList().contains(store))   stealInfoList.add(info);
  return stealInfoList;
}
