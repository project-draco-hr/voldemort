{
  for (  int p : partitionIds) {
    for (    RebalancePartitionsInfoLiveCycle info : getAll()) {
      if (info.getRebalancePartitionsInfo().getUnbalancedStoreList().contains(store) && info.getRebalancePartitionsInfo().getPartitionList().contains(p))       return info;
    }
  }
  return null;
}
