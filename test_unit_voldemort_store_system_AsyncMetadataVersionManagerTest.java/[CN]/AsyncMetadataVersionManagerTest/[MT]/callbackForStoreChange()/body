{
  try {
    Long storeVersion=this.asyncCheckMetadata.getStoreMetadataVersion();
    if (storeVersion != null) {
      this.updatedStoreVersion=storeVersion;
    }
  }
 catch (  Exception e) {
    fail("Error in updating store version: " + e.getMessage());
  }
 finally {
    this.callbackDone=true;
  }
}
