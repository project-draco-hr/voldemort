{
  this.signal=signal;
  this.opCode=opCode;
  this.expectedTimeMs=-1;
  this.runTimes=null;
  this.key=new ByteArray(ByteUtils.getBytes("key","UTF-8"));
  this.value=ByteUtils.getBytes("value","UTF-8");
  logger.debug("OpInvoker created for operation " + getOpName() + "(Thread: "+ Thread.currentThread().getName()+ ")");
}
