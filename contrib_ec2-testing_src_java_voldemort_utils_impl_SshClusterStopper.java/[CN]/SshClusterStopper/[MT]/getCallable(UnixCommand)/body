{
  if (suppressErrors) {
    final CommandOutputListener commandOutputListener=new LoggingCommandOutputListener(null,logger,true);
    return new Callable<Object>(){
      public Object call() throws Exception {
        try {
          command.execute(commandOutputListener);
        }
 catch (        Exception e) {
        }
        return null;
      }
    }
;
  }
 else {
    return super.getCallable(command);
  }
}
