{
  StorageConfiguration conf=new BdbStorageConfiguration(new VoldemortConfig(propsA));
  Store<String,String> store=new SerializingStore<String,String>(conf.getStore("test" + 0),new StringSerializer(),new StringSerializer());
  InconsistencyResolver<Versioned<String>> resolver=new VectorClockInconsistencyResolver<String>();
  return new DefaultStoreClient<String,String>(new InconsistencyResolvingStore<String,String>(store,resolver),new StringSerializer(),new StringSerializer(),null);
}
