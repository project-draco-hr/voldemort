def _maybe_reconnect(self):
    if (self.request_count >= self.reconnect_interval):
        self.connection.close()
        (self.node_id, self.connection) = self._connect_to_random()
        logging.debug(((((('Closing connection to node ' + str(self.node_id)) + ' after ') + str(self.request_count)) + ' requests and reconnecting to node ') + str(self.node_id)))
        self.request_count = 0
