{
  try {
    List<String> instanceIds=file.exists() ? FileUtils.readLines(file) : new ArrayList<String>();
    instanceIds.add(instanceId);
    FileUtils.writeLines(file,instanceIds);
    if (logger.isInfoEnabled())     logger.info("Instances created: " + FileUtils.readLines(file));
  }
 catch (  Exception e) {
    if (logger.isEnabledFor(Level.ERROR))     logger.error(e,e);
  }
}
