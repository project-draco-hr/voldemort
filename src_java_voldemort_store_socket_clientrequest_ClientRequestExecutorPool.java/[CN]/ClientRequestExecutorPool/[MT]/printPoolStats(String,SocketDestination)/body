{
  if (logger.isDebugEnabled()) {
    logger.debug("CREP::" + tag + " : "+ destination.toString()+ " --- AQ QLen = "+ queuedPool.getRegisteredResourceRequestCount(destination)+ " --- SQ QLen = "+ queuedPool.getBlockingGetsCount(destination)+ ", ChkIn = "+ queuedPool.getCheckedInResourcesCount(destination)+ ", Tot = "+ queuedPool.getTotalResourceCount(destination));
  }
}
