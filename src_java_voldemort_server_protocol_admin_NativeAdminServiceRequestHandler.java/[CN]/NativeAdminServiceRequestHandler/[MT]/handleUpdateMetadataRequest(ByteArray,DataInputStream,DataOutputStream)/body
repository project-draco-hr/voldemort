{
  String keyString=ByteUtils.getString(key.get(),"UTF-8");
  if (MetadataStore.METADATA_KEYS.contains(keyString)) {
    int valueSize=inputStream.readInt();
    byte[] value=new byte[valueSize];
    ByteUtils.read(inputStream,value);
    VectorClock clock=new VectorClock(value);
    String valueString=ByteUtils.getString(ByteUtils.copy(value,clock.sizeInBytes(),value.length),"UTF-8");
    Versioned<byte[]> versionedValue=new Versioned<byte[]>(ByteUtils.getBytes(valueString,"UTF-8"),clock);
    getMetadataStore().put(new ByteArray(ByteUtils.getBytes(keyString,"UTF-8")),versionedValue);
    outputStream.writeInt(-1);
    outputStream.flush();
  }
 else {
    writeException(outputStream,new VoldemortException("Metadata Key passed " + keyString + " is not handled yet ..."));
  }
}
