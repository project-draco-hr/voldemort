{
  if (latency >= _buckets) {
    histogramOverflow++;
  }
 else {
    histogram[latency]++;
  }
  operations++;
  totalLatency+=latency;
  windowOperations++;
  windowTotalLatency+=latency;
  if ((minLatency < 0) || (latency < minLatency)) {
    minLatency=latency;
  }
  if ((maxLatency < 0) || (latency > maxLatency)) {
    maxLatency=latency;
  }
}
