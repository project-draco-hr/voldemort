{
  stealInfo.setAttempt(stealInfo.getAttempt() + 1);
  VectorClock currentVersion=(VectorClock)metadata.get(MetadataStore.REBALANCING_STEAL_INFO).get(0).getVersion();
  metadata.put(MetadataStore.REBALANCING_STEAL_INFO,new Versioned<Object>(stealInfo,currentVersion.incremented(metadata.getNodeId(),System.currentTimeMillis())));
  return stealInfo;
}
