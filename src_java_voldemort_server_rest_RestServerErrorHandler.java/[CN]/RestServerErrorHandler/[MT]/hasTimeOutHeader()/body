{
  boolean result=false;
  String timeoutValStr=this.request.getHeader(RestMessageHeaders.X_VOLD_REQUEST_TIMEOUT_MS);
  if (timeoutValStr != null) {
    try {
      this.parsedRoutingTimeoutInMs=Long.parseLong(timeoutValStr);
      result=true;
    }
 catch (    NumberFormatException nfe) {
      writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Incorrect timeout parameter. Cannot parse this to long: " + timeoutValStr);
      result=false;
    }
  }
 else {
    writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Missing timeout parameter.");
    result=false;
  }
  return result;
}
