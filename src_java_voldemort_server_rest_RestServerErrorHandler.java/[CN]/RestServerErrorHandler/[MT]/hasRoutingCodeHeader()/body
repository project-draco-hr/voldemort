{
  String rtCode=this.request.getHeader(RestMessageHeaders.X_VOLD_ROUTING_TYPE_CODE);
  boolean result=false;
  if (rtCode != null) {
    try {
      int routingTypeCode=Integer.parseInt(rtCode);
      this.parsedRoutingType=RequestRoutingType.getRequestRoutingType(routingTypeCode);
      result=true;
    }
 catch (    NumberFormatException nfe) {
      writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Incorrect routing type parameter. Cannot parse this to long: " + rtCode);
      result=false;
    }
catch (    VoldemortException ve) {
      writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Incorrect routing type code: " + rtCode);
      result=false;
    }
  }
 else {
    writeErrorResponse(this.messageEvent,HttpResponseStatus.BAD_REQUEST,"Missing routing type parameter.");
    result=false;
  }
  return result;
}
