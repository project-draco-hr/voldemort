{
  if (this == object)   return true;
  if (object == null)   return false;
  if (!object.getClass().equals(ClientInfo.class))   return false;
  ClientInfo clientInfo=(ClientInfo)object;
  return (this.bootstrapTime == clientInfo.bootstrapTime) && (this.context.equals(clientInfo.context)) && (this.deploymentPath.equals(clientInfo.deploymentPath))&& (this.localHostName.equals(clientInfo.localHostName))&& (this.sequence == clientInfo.sequence)&& (this.storeName.equals(clientInfo.storeName))&& (this.updateTime == clientInfo.updateTime)&& (this.releaseVersion == clientInfo.releaseVersion);
}
