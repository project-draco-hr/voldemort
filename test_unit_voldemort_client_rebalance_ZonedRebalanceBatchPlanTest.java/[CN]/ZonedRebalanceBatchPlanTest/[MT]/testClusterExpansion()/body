{
  List<RebalanceTaskInfo> batchPlan;
  batchPlan=ClusterTestUtils.getBatchPlan(zzClusterExpansionNN,zzClusterExpansionPP,zzStores);
  assertTrue(batchPlan.size() > 0);
  batchPlan=ClusterTestUtils.getBatchPlan(zzzClusterExpansionNNN,zzzClusterExpansionPPP,zzzStores);
  assertTrue(batchPlan.size() > 0);
}
