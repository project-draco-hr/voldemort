{
  boolean belongs=false;
  List<Integer> nodePartitions=cluster.getNodeById(nodeId).getPartitionIds();
  List<Integer> replicatingPartitions=new RoutingStrategyFactory().updateRoutingStrategy(storeDef,cluster).getReplicatingPartitionList(partitionId);
  if (replicaType < replicatingPartitions.size()) {
    if (nodePartitions.contains(replicatingPartitions.get(replicaType)))     belongs=true;
  }
  return belongs;
}
