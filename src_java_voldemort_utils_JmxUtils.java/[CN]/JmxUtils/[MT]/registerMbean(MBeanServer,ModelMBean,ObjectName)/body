{
  try {
    server.registerMBean(mbean,name);
  }
 catch (  InstanceAlreadyExistsException e) {
    throw new VoldemortException(e);
  }
catch (  MBeanRegistrationException e) {
    throw new VoldemortException(e);
  }
catch (  NotCompliantMBeanException e) {
    throw new VoldemortException(e);
  }
}
