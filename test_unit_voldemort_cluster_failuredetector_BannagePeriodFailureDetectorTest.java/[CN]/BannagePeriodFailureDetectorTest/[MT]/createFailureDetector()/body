{
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(BannagePeriodFailureDetector.class.getName()).setBannagePeriod(BANNAGE_MILLIS).setCluster(cluster).setStoreVerifier(create(cluster.getNodes())).setTime(time);
  return create(failureDetectorConfig,true);
}
