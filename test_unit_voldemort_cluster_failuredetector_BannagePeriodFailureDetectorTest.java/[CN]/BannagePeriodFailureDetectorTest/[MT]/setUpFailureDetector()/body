{
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(BannagePeriodFailureDetector.class.getName()).setNodeBannagePeriod(BANNAGE_MILLIS).setNodes(cluster.getNodes()).setStoreResolver(createMutableStoreResolver(cluster.getNodes())).setTime(time);
  return create(failureDetectorConfig);
}
