{
  for (  SystemStoreConstants.SystemStoreName storeName : SystemStoreConstants.SystemStoreName.values()) {
    SystemStore sysStore=new SystemStore(storeName.name(),config.getBootstrapUrls(),config.getClientZoneId(),clusterXml,fd,config,this.socketStoreFactory);
    this.sysStoreMap.put(storeName.name(),sysStore);
  }
}
