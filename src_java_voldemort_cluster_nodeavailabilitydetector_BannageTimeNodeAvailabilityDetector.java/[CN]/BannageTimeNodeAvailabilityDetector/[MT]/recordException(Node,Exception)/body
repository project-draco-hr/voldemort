{
  getNodeStatus(node).setUnavailable();
  if (logger.isEnabledFor(Level.WARN))   logger.warn("Could not connect to node " + node.getId() + " at "+ node.getHost()+ " marking as unavailable for "+ nodeAvailabilityDetectorConfig.getNodeBannagePeriod()+ " ms.",e);
  if (logger.isDebugEnabled())   logger.debug(e);
}
