{
  if (outputType == OutputType.STDERR) {
    if (logger.isWarnEnabled())     logger.warn("ERROR from " + hostName + ": "+ line);
  }
 else   if (outputType == OutputType.STDOUT) {
    if (logger.isInfoEnabled())     logger.info("From " + hostName + ": "+ line);
  }
  if (delegate != null)   delegate.outputReceived(outputType,hostName,line);
}
