{
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(ThresholdFailureDetector.class.getName()).setBannagePeriod(BANNAGE_MILLIS).setNodes(cluster.getNodes()).setStoreResolver(createMutableStoreResolver(cluster.getNodes())).setTime(time);
  return create(failureDetectorConfig);
}
