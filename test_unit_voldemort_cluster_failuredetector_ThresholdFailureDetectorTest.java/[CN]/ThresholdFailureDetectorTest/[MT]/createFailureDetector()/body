{
  storeVerifier=create(cluster.getNodes());
  FailureDetectorConfig failureDetectorConfig=new FailureDetectorConfig().setImplementationClassName(ThresholdFailureDetector.class.getName()).setBannagePeriod(BANNAGE_MILLIS).setAsyncRecoveryInterval(250).setThresholdInterval(500).setNodes(cluster.getNodes()).setStoreVerifier(storeVerifier).setTime(time);
  return create(failureDetectorConfig,true);
}
