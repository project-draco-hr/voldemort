{
  StoreUtils.assertValidKeys(keys);
  HttpPost method=null;
  try {
    method=new HttpPost(this.storeUrl);
    ByteArrayOutputStream outputBytes=new ByteArrayOutputStream();
    requestFormat.writeGetAllRequest(new DataOutputStream(outputBytes),storeName,keys,transforms,reroute);
    DataInputStream input=executeRequest(method,outputBytes);
    return requestFormat.readGetAllResponse(input);
  }
 catch (  IOException e) {
    throw new UnreachableStoreException("Could not connect to " + storeUrl + " for "+ storeName,e);
  }
}
