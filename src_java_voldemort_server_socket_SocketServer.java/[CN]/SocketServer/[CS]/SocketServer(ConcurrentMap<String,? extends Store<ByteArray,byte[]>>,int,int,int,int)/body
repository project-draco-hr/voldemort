{
  this.port=port;
  this.socketBufferSize=socketBufferSize;
  this.threadGroup=new ThreadGroup("voldemort-socket-server");
  this.storeMap=storeMap;
  this.threadPool=new ThreadPoolExecutor(defaultThreads,maxThreads,1,TimeUnit.MILLISECONDS,new SynchronousQueue<Runnable>(),threadFactory,rejectedExecutionHandler);
