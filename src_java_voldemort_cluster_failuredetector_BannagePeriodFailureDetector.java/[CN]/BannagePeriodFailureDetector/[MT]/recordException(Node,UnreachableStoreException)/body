{
  if (e != null) {
    if (logger.isEnabledFor(Level.WARN))     logger.warn(e,e);
  }
  if (logger.isEnabledFor(Level.WARN))   logger.warn("Could not connect to node " + node.getId() + " at "+ node.getHost()+ " marking as unavailable for "+ getConfig().getNodeBannagePeriod()+ " ms.");
  setUnavailable(node);
}
