{
  assertEquals("Versions of put did not advance",Occured.AFTER,newVersion.compare(oldVersion));
  assertEquals("Version of put is larger than version of get",Occured.BEFORE,newVersion.compare(getVersioned.getVersion()));
  assertEquals("Version of put is smaller than version of get",Occured.BEFORE,getVersioned.getVersion().compare(newVersion));
  assertEquals("Value of put does not match value of get",newValue,getVersioned.getValue());
}
