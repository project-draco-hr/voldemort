{
  String externalHostName=instance.getDnsName() != null ? instance.getDnsName().trim() : "";
  String internalHostName=instance.getPrivateDnsName() != null ? instance.getPrivateDnsName().trim() : "";
  if (externalHostName.length() == 0 || internalHostName.length() == 0)   return null;
  return new HostNamePair(externalHostName,internalHostName);
}
