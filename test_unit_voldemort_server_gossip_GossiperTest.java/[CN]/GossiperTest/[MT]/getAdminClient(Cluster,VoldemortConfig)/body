{
  ClientConfig clientConfig=new ClientConfig().setMaxConnectionsPerNode(8).setMaxThreads(8).setConnectionTimeout(newServerConfig.getAdminConnectionTimeout(),TimeUnit.MILLISECONDS).setSocketTimeout(newServerConfig.getSocketTimeoutMs(),TimeUnit.MILLISECONDS).setSocketBufferSize(newServerConfig.getAdminSocketBufferSize());
  return new ProtoBuffAdminClientRequestFormat(newCluster,clientConfig);
}
