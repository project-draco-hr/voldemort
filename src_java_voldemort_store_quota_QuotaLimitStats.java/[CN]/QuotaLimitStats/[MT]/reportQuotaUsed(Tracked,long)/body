{
  if (Tracked.GET == op) {
    reportGetQuotaUsedPct(pctUsed);
  }
 else   if (Tracked.PUT == op) {
    reportPutQuotaUsedPct(pctUsed);
  }
 else   if (Tracked.GET_ALL == op) {
    reportGetAllQuotaUsedPct(pctUsed);
  }
 else   if (Tracked.DELETE == op) {
    reportDeleteQuotaUsedPct(pctUsed);
  }
}
