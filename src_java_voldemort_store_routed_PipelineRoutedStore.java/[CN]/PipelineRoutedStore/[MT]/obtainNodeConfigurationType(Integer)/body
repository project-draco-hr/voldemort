{
  if (zonesRequired != null && routingStrategy.getType().equals(RoutingStrategyType.TO_ALL_LOCAL_PREF_STRATEGY))   return ConfigureNodesType.BYZONE_LOCAL;
 else   if (zonesRequired == null && routingStrategy.getType().equals(RoutingStrategyType.TO_ALL_LOCAL_PREF_STRATEGY))   return ConfigureNodesType.DEFAULT_LOCAL;
 else   if (zonesRequired != null && !routingStrategy.getType().equals(RoutingStrategyType.TO_ALL_LOCAL_PREF_STRATEGY))   return ConfigureNodesType.BYZONE;
  return ConfigureNodesType.DEFAULT;
}
