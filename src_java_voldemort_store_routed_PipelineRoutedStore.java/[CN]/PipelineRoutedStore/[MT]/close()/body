{
  VoldemortException exception=null;
  for (  NonblockingStore store : nonblockingStores.values()) {
    try {
      store.close();
    }
 catch (    VoldemortException e) {
      exception=e;
    }
  }
  if (this.jmxEnabled) {
    JmxUtils.unregisterMbean(JmxUtils.createObjectName(JmxUtils.getPackageName(stats.getClass()),getName() + JmxUtils.getJmxId(jmxId)));
  }
  if (exception != null)   throw exception;
  super.close();
}
