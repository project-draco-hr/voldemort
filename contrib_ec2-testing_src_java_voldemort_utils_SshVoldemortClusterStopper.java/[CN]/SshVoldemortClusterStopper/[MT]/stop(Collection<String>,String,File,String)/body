{
  try {
    List<UnixCommand> unixCommands=generateCommands("SshVoldemortClusterStopper.ssh",hostNames,hostUserId,sshPrivateKey,voldemortRootDirectory,null,null);
    for (    UnixCommand unixCommand : unixCommands)     unixCommand.execute();
  }
 catch (  InterruptedException e) {
    throw new VoldemortStartClusterException(e);
  }
catch (  IOException e) {
    throw new VoldemortStartClusterException(e);
  }
}
