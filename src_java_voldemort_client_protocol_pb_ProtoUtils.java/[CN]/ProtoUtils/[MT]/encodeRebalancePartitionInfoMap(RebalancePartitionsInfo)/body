{
  return RebalancePartitionInfoMap.newBuilder().setStealerId(stealInfo.getStealerId()).setDonorId(stealInfo.getDonorId()).addAllReplicaToPartition(ProtoUtils.encodePartitionTuple(stealInfo.getReplicaToPartitionList())).addAllUnbalancedStores(stealInfo.getUnbalancedStoreList()).setInitialCluster(new ClusterMapper().writeCluster(stealInfo.getInitialCluster())).setAttempt(stealInfo.getAttempt()).setDeletePartitions(stealInfo.getDeletePartitions()).build();
}
