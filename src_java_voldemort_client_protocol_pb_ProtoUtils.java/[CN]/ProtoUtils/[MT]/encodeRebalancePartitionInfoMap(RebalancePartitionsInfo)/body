{
  return RebalancePartitionInfoMap.newBuilder().setStealerId(stealInfo.getStealerId()).setDonorId(stealInfo.getDonorId()).addAllReplicaToAddPartition(ProtoUtils.encodePerStorePartitionTuple(stealInfo.getStoreToReplicaToAddPartitionList())).setInitialCluster(new ClusterMapper().writeCluster(stealInfo.getInitialCluster())).build();
}
