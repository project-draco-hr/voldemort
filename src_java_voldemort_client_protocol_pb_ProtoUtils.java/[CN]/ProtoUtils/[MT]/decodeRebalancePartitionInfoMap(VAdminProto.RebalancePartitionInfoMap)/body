{
  RebalancePartitionsInfo rebalanceStealInfo=new RebalancePartitionsInfo(rebalancePartitionInfoMap.getStealerId(),rebalancePartitionInfoMap.getDonorId(),decodePerStorePartitionTuple(rebalancePartitionInfoMap.getReplicaToAddPartitionList()),decodePerStorePartitionTuple(rebalancePartitionInfoMap.getReplicaToDeletePartitionList()),new ClusterMapper().readCluster(new StringReader(rebalancePartitionInfoMap.getInitialCluster())),rebalancePartitionInfoMap.getAttempt());
  return rebalanceStealInfo;
}
