{
  HashMap<Integer,List<Integer>> replicaToPartitionList=decodePartitionTuple(rebalancePartitionInfoMap.getReplicaToPartitionList());
  RebalancePartitionsInfo rebalanceStealInfo=new RebalancePartitionsInfo(rebalancePartitionInfoMap.getStealerId(),rebalancePartitionInfoMap.getDonorId(),replicaToPartitionList,rebalancePartitionInfoMap.getUnbalancedStoresList(),new ClusterMapper().readCluster(new StringReader(rebalancePartitionInfoMap.getInitialCluster())),rebalancePartitionInfoMap.getDeletePartitions(),rebalancePartitionInfoMap.getAttempt());
  return rebalanceStealInfo;
}
