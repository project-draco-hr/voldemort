{
  RebalancePartitionsInfo rebalanceStealInfo=new RebalancePartitionsInfo(rebalancePartitionInfoMap.getStealerId(),rebalancePartitionInfoMap.getDonorId(),decodePerStorePartitionTuple(rebalancePartitionInfoMap.getReplicaToAddPartitionList()),new ClusterMapper().readCluster(new StringReader(rebalancePartitionInfoMap.getInitialCluster())));
  return rebalanceStealInfo;
}
