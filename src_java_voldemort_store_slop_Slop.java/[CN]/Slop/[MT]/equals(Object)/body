{
  if (obj == null)   return false;
 else   if (!obj.getClass().equals(Slop.class))   return false;
  Slop slop=(Slop)obj;
  return operation == slop.getOperation() && Objects.equal(storeName,getStoreName()) && key.equals(slop.getKey()) && Objects.deepEquals(value,slop.getValue()) && nodeId == slop.getNodeId() && Objects.equal(arrived,slop.getArrived());
}
