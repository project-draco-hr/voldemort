{
  V resource=null;
  if (connectionsInProgress.get() == 0) {
    resource=pool.nonBlockingGet();
  }
  if (resource == null) {
    connectionsInProgress.incrementAndGet();
    try {
      attemptGrow(key,this.objectFactory,pool);
      resource=pool.nonBlockingGet();
    }
  finally {
      connectionsInProgress.decrementAndGet();
    }
  }
  return resource;
}
