{
  this.port=ServerTestUtils.findFreePort();
  this.pool=new SocketPool(maxConnectionsPerNode,maxTotalConnections,1000,1000,32 * 1024);
  this.dest1=new SocketDestination("localhost",port,RequestFormatType.VOLDEMORT_V1);
  RequestHandlerFactory handlerFactory=new RequestHandlerFactory(new StoreRepository(),null,null);
  this.server=new SocketServer("test-socket",port,maxTotalConnections,maxTotalConnections + 3,10000,handlerFactory);
  this.server.start();
  this.server.awaitStartupCompletion();
}
