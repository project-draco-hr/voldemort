{
  this(null,null,config.getHdfsReportingIntervalBytes(),config.getFetcherBufferSize(),config.getHdfsMinBytesPerSecond(),config.getReadOnlyKeytabPath(),config.getReadOnlyKerberosUser());
  this.voldemortConfig=config;
  logger.info("Created hdfs fetcher with no dynamic throttler, buffer size " + bufferSize + ", reporting interval bytes "+ reportingIntervalBytes);
}
