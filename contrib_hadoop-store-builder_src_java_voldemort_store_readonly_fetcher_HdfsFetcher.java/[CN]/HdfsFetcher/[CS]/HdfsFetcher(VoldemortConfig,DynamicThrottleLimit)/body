{
  this(dynThrottleLimit,null,config.getHdfsReportingIntervalBytes(),config.getFetcherBufferSize(),config.getHdfsMinBytesPerSecond(),config.getReadOnlyKeytabPath(),config.getReadOnlyKerberosUser());
  this.voldemortConfig=config;
  logger.info("Created hdfs fetcher with throttle rate " + dynThrottleLimit.getRate() + ", buffer size "+ bufferSize+ ", reporting interval bytes "+ reportingIntervalBytes);
}
