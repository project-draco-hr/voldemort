{
  this(dynThrottleLimit,null,config.getReportingIntervalBytes(),config.getFetcherBufferSize(),config.getMinBytesPerSecond(),config.getReadOnlyKeytabPath(),config.getReadOnlyKerberosUser());
  this.voldemortConfig=config;
  logger.info("Created hdfs fetcher with throttle rate " + dynThrottleLimit.getRate() + ", buffer size "+ bufferSize+ ", reporting interval bytes "+ reportingIntervalBytes);
}
