{
  this(props.containsKey("fetcher.max.bytes.per.sec") ? props.getBytes("fetcher.max.bytes.per.sec") : null,props.getBytes("fetcher.reporting.interval.bytes",REPORTING_INTERVAL_BYTES),(int)props.getBytes("hdfs.fetcher.buffer.size",DEFAULT_BUFFER_SIZE));
