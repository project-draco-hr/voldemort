{
  this(dynThrottleLimit,props.getBytes("fetcher.reporting.interval.bytes",REPORTING_INTERVAL_BYTES),(int)props.getBytes("hdfs.fetcher.buffer.size",DEFAULT_BUFFER_SIZE));
  logger.info("Created hdfs fetcher with throttle rate " + dynThrottleLimit.getRate() + ", buffer size "+ bufferSize+ ", reporting interval bytes "+ reportingIntervalBytes);
}
