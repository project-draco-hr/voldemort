{
  this(dynThrottleLimit,props.getBytes("fetcher.reporting.interval.bytes",REPORTING_INTERVAL_BYTES),(int)props.getBytes("hdfs.fetcher.buffer.size",DEFAULT_BUFFER_SIZE),props.containsKey("fetcher.min.bytes.per.sec") ? props.getBytes("fetcher.min.bytes.per.sec") : 0);
  logger.info("Created hdfs fetcher with throttle rate " + dynThrottleLimit.getRate() + ", buffer size "+ bufferSize+ ", reporting interval bytes "+ reportingIntervalBytes);
}
